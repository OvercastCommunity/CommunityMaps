        <item slot="32" material="quartz block" amount="64"/>
    </kit>
    <kit id="red-kit" parents="spawn-kit">
        <helmet material="leather helmet" color="993333" unbreakable="true"/>
        <chestplate material="leather chestplate" color="993333" enchantment="protection projectile" unbreakable="true"/>
        <leggings material="leather leggings" color="993333" unbreakable="true"/>
        <boots material="leather boots" color="993333" unbreakable="true"/>
