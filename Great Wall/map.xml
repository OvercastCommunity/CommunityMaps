            <everywhere/>
        </region>
        <renew-filter>
            <any>
                <material>iron block</material>
            </any>
        </renew-filter>
    </renewable>
</renewables>
<filters>
    <team id="only-red">red-team</team>
    <team id="only-blue">blue-team</team>
    <not id="not-void">
        <void/>
    </not>
    <any id="prot-spawn">
        <material>iron block</material>
        <material>diamond block</material>
    </any>
    <any id="prot-wool">
        <material>web</material>
        <material>stained glass pane</material>
    </any>
</filters>
