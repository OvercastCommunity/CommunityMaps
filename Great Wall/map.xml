            <block>93.5,29.5,-76.5</block>
            <block>93.5,29.5,-54.5</block>
        </regions>
    </spawn>
    <default>
        <regions yaw="90">
            <cuboid min="125,42,-13" max="125,42,-14"/>
        </regions>
    </default>
</spawns>
<wools craftable="false">
    <wool team="red-team" color="brown" location="14.5,51,-171.5">
