<?xml version="1.0" encoding="utf-8"?>
<map proto="1.4.0">
<name>Two-Quarter</name>
<version>1.2</version>
<objective>Capture the wools located on the enemy sides!</objective>
<authors>
    <author uuid="5ebc601c-82b6-4cd2-88cf-1b5c552b8ace"/><!--Pe241-->
</authors>
<teams>
    <team id="cyan" color="dark aqua" max="24">Cyan</team>
    <team id="magenta" color="light purple" max="24">Magenta</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item unbreakable="true" slot="0" material="iron sword"/>
        <item unbreakable="true" slot="1" enchantment="arrow infinite" material="bow"/>
        <item unbreakable="true" slot="2" material="diamond pickaxe"/>
        <item unbreakable="true" slot="3" material="iron axe"/>
        <item slot="4" material="golden apple"/>
        <item slot="9" material="arrow"/>
        <potion amplifier="50" duration="oo">regeneration</potion>
    </kit>
    <kit id="cyan-kit" parents="spawn-kit">
        <helmet color="4c7f99" unbreakable="true" material="leather helmet"/>
        <chestplate color="4c7f99" unbreakable="true" material="leather chestplate"/>
        <leggings color="4c7f99" unbreakable="true" material="leather leggings"/>
        <boots color="4c7f99" unbreakable="true" material="leather boots"/>
        <item slot="5" amount="64" damage="9" material="stained glass"/>
        <item slot="6" amount="64" damage="0" material="log"/>
    </kit>
    <kit id="magenta-kit" parents="spawn-kit">
        <helmet color="b24cd8" unbreakable="true" material="leather helmet"/>
        <chestplate color="b24cd8" unbreakable="true" material="leather chestplate"/>
        <leggings color="b24cd8" unbreakable="true" material="leather leggings"/>
        <boots color="b24cd8" unbreakable="true" material="leather boots"/>
        <item slot="5" amount="64" damage="2" material="stained glass"/>
        <item slot="6" amount="64" damage="1" material="log"/>
    </kit>
    <kit id="regeneration" force="true">
        <potion amplifier="50" duration="oo">regeneration</potion>
    </kit>
    <kit id="respawnkill" force="true">
        <potion amplifier="50" duration="3">damage resistance</potion>
        <potion amplifier="50" duration="3">regeneration</potion>
    </kit>
</kits>
<kill-rewards>
    <kill-reward>
        <item>golden apple</item>
    </kill-reward>
</kill-rewards>
<wools>
    <wool team="cyan" color="red" location="55.5,18.5,-54.5">
        <monument>
            <block>8,44,-8</block>
        </monument>
    </wool>
    <wool team="cyan" color="pink" location="-54.5,18.5,55.5">
        <monument>
            <block>-8,44,8</block>
        </monument>
    </wool>
    <wool team="magenta" color="blue" location="-54.5,18.5,-54.5">
        <monument>
            <block>-8,44,-8</block>
        </monument>
    </wool>
    <wool team="magenta" color="light_blue" location="55.5,18.5,55.5">
        <monument>
            <block>8,44,8</block>
        </monument>
    </wool>
</wools>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather boots</item>
    <item>leather leggings</item>
    <item>string</item>
    <item>torch</item>
</itemremove>
<itemkeep>
    <item>iron sword</item>
    <item>bow</item>
    <item>diamond pickaxe</item>
    <item>iron axe</item>
    <item>iron helmet</item>
    <item>iron chestplate</item>
    <item>iron leggings</item>
    <item>iron boots</item>
    <item>golden apple</item>
    <item>log</item>
    <item>wood</item>
    <item>stained glass</item>
    <item>arrow</item>
</itemkeep>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>arrow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<filters>
    <team id="only-cyan">cyan</team>
    <team id="only-magenta">magenta</team>
    <deny id="deny-void">
        <void/>
    </deny>
    <deny id="allow-iron">
        <not><material>iron block</material></not>        
    </deny>
    <deny id="allow-woolroom-cyan">
        <any>
            <team>cyan</team>
            <all>
                <team>magenta</team>
                <not>
                    <any>
                        <material>wool</material>
                        <material>web</material>
                    </any>
                </not>
            </all>
        </any>
    </deny>
    <deny id="allow-woolroom-magenta">
        <any>
            <team>magenta</team>
            <all>
                <team>cyan</team>
                <not>
                    <any>
                        <material>wool</material>
                        <material>web</material>
                    </any>
                </not>
            </all>
        </any>
    </deny>
    <deny id="deny-player">
        <cause>player</cause>
    </deny>
</filters>
<regions>
    <cylinder id="center" base="0.5,40,0.5" radius="17" height="oo"/>
    <negative id="no-center">
        <region id="center"/>
    </negative>
    <union id="lightblue-exception">
        <rectangle min="48,48" max="53,49"/>
        <rectangle min="48,48" max="49,53"/>
        <rectangle min="49,49" max="51,50"/>
        <rectangle min="49,49" max="50,51"/>
        
        <rectangle min="60,61" max="62,62"/>
        <rectangle min="61,60" max="62,62"/>
        <rectangle min="58,62" max="63,63"/>
        <rectangle min="62,58" max="63,63"/>
    
        <rectangle min="60,49" max="62,50"/>
        <rectangle min="61,50" max="62,51"/>
        <rectangle min="58,48" max="63,49"/>
        <rectangle min="62,49" max="63,52"/>
    
        <rectangle min="49,60" max="50,62"/>
        <rectangle min="50,61" max="51,62"/>
        <rectangle min="48,58" max="49,63"/>
        <rectangle min="49,62" max="53,63"/>
    </union>
    <union id="blue-exception">
        <rectangle min="-62,-52" max="-61,-47"/>
        <rectangle min="-61,-48" max="-57,-47"/>
        <rectangle min="-61,-50" max="-60,-48"/>
        <rectangle min="-60,-49" max="-59,-48"/>
    
        <rectangle min="-48,-52" max="-47,-47"/>
        <rectangle min="-52,-48" max="-47,-47"/>
        <rectangle min="-50,-49" max="-48,-48"/>
        <rectangle min="-49,-50" max="-48,-49"/>
        
        <rectangle min="-62,-62" max="-57,-61"/>
        <rectangle min="-62,-62" max="-61,-57"/>
        <rectangle min="-61,-61" max="-59,-60"/>
        <rectangle min="-61,-61" max="-60,-59"/>
    
        <rectangle min="-52,-62" max="-47,-61"/>
        <rectangle min="-48,-62" max="-47,-57"/>
        <rectangle min="-50,-61" max="-48,-60"/>
        <rectangle min="-49,-60" max="-48,-59"/>
    </union>
    <union id="red-exception">
        <rectangle min="48,-62" max="53,-61"/>
        <rectangle min="48,-62" max="49,-57"/>
        <rectangle min="49,-61" max="51,-60"/>
        <rectangle min="49,-61" max="50,-59"/>
            
        <rectangle min="58,-62" max="63,-61"/>
        <rectangle min="62,-61" max="63,-57"/>
        <rectangle min="60,-61" max="62,-60"/>
        <rectangle min="61,-60" max="62,-59"/>
    
        <rectangle min="58,-48" max="63,-47"/>
        <rectangle min="62,-52" max="63,-47"/>
        <rectangle min="61,-50" max="62,-48"/>
        <rectangle min="60,-49" max="62,-48"/>
    
        <rectangle min="48,-52" max="49,-47"/>
        <rectangle min="49,-48" max="53,-47"/>
        <rectangle min="49,-50" max="50,-48"/>
        <rectangle min="50,-49" max="51,-48"/>
    </union>
    <union id="pink-exception">
        <rectangle min="-62,48" max="-57,49"/>
        <rectangle min="-62,48" max="-61,53"/>
        <rectangle min="-61,49" max="-59,50"/>
        <rectangle min="-61,49" max="-60,51"/>
            
        <rectangle min="-62,58" max="-61,63"/>
        <rectangle min="-61,62" max="-57,63"/>
        <rectangle min="-61,60" max="-60,62"/>
        <rectangle min="-60,61" max="-59,62"/>
    
        <rectangle min="-52,48" max="-47,49"/>
        <rectangle min="-48,49" max="-47,53"/>
        <rectangle min="-50,49" max="-48,50"/>
        <rectangle min="-49,50" max="-48,51"/>
    
        <rectangle min="-48,58" max="-47,63"/>
        <rectangle min="-52,62" max="-47,63"/>
        <rectangle min="-50,61" max="-48,62"/>
        <rectangle min="-49,60" max="-48,62"/>
    </union>
    <union id="bases">
        <union id="cyan-bases">
            <complement id="lightblue-base">
                <cuboid min="48,0,48" max="63,15,63"/>
                <region id="lightblue-exception"/>
            </complement>
            <complement id="blue-base">
                <cuboid min="-62,0,-62" max="-47,15,-47"/>
                <region id="blue-exception"/>
            </complement>
        </union>
        <union id="magenta-bases">
            <complement id="red-base">
                <cuboid min="48,0,-62" max="63,15,-47"/>
                <region id="red-exception"/>
            </complement>
            <complement id="pink-base">
                <cuboid min="-62,0,48" max="-47,15,63"/>
                <region id="pink-exception"/>
            </complement>
        </union>
    </union>
    <union id="woolrooms">
        <union id="cyan-woolrooms">
            <complement id="lightblue-woolroom">
                <cuboid min="48,15,48" max="63,oo,63"/>
                <region id="lightblue-exception"/>
            </complement>
            <complement id="blue-woolroom">
                <cuboid min="-62,15,-62" max="-47,oo,-47"/>
                <region id="blue-exception"/>
            </complement>
        </union>
        <union id="magenta-woolrooms">
            <complement id="red-woolroom">
                <cuboid min="48,15,-62" max="63,oo,-47"/>
                <region id="red-exception"/>
            </complement>
            <complement id="pink-woolroom">
                <cuboid min="-62,15,48" max="-47,oo,63"/>
                <region id="pink-exception"/>
            </complement>
        </union>
    </union>
    <union id="backportal">
        <cuboid min="-55,2,55" max="-54,4,56"/>
        <cuboid min="-55,2,-55" max="-54,4,-54"/>
        <cuboid min="55,2,-55" max="56,4,-54"/>
        <cuboid min="55,2,55" max="56,4,56"/>
    </union>
    <union id="cyan-portals">
        <cuboid id="blue-portal" min="-5,43,-5" max="-4,45,-4"/>
        <cuboid id="lightblue-portal" min="5,43,5" max="6,45,6"/>
    </union>
    <union id="magenta-portals">
        <cuboid id="red-portal" min="5,43,-5" max="6,45,-4"/>
        <cuboid id="pink-portal" min="-5,43,5" max="-4,45,6"/>
    </union>
    <negative id="yes-void">
        <rectangle min="-25,55" max="26,101"/>
        <rectangle min="55,-25" max="101,26"/>
        <rectangle min="-25,-100" max="26,-54"/>
        <rectangle min="-100,-25" max="-54,26"/>
        <block>8,44,-8</block>
        <block>-8,44,8</block>
        <block>-8,44,-8</block>
        <block>8,44,8</block>
    </negative>
    <complement id="maxbuildheight">
        <above y="35"/>
        <region id="center"/>
    </complement>

    <apply block="deny-void" region="yes-void" message="You can't edit here!"/>
    <apply block="never" region="maxbuildheight" message="You have reached the maximum build height"/>
    <apply block-break="allow-iron" region="bases" message="You can only break iron blocks in the base!"/>
    <apply block-place="deny-player" region="bases" message="You can't place blocks in the base!"/>
    <apply block-break="allow-woolroom-cyan" region="cyan-woolrooms" message="You can't break this!"/>
    <apply block-break="allow-woolroom-magenta" region="magenta-woolrooms" message="You can't break this!"/>
    <apply block-place="deny-player" region="woolrooms" message="You can't place blocks in the woolroom!"/>
    <apply enter="only-cyan" region="cyan-bases" message="You can't enter the enemy's base!"/>
    <apply enter="only-magenta" region="magenta-bases" message="You can't enter the enemy's base!"/>
    <apply enter="only-cyan" region="cyan-portals" message="This is the `3Cyan`c-only portal!"/>
    <apply enter="only-magenta" region="magenta-portals" message="This is the `dMagenta`c-only portal!"/>
    <apply enter="only-cyan" region="magenta-woolrooms" message="You can't enter your own wool room!"/>
    <apply enter="only-magenta" region="cyan-woolrooms" message="You can't enter your own wool room!"/>
    <apply kit="regeneration" region="center"/>
    <apply kit="respawnkill" region="no-center"/>
</regions>
<spawns>
    <default>
        <regions>
            <point yaw="45">0,44,1</point>
            <point yaw="135">0,44,0</point>
            <point yaw="-135">1,44,0</point>
            <point yaw="-45">1,44,1</point>
        </regions>
    </default>
    <spawn team="magenta" kit="magenta-kit">
        <regions>
            <point yaw="0">0.5,44,1.5</point>
            <point yaw="-90">1.5,44,0.5</point>
            <point yaw="180">0.5,44,-0.5</point>
            <point yaw="90">-0.5,44,0.5</point>
        </regions>
    </spawn>
    <spawn team="cyan" kit="cyan-kit">
        <regions>
            <point yaw="0">0.5,44,1.5</point>
            <point yaw="-90">1.5,44,0.5</point>
            <point yaw="180">0.5,44,-0.5</point>
            <point yaw="90">-0.5,44,0.5</point>
        </regions>
    </spawn>
</spawns>
<portals>
    <portal region="backportal" x="@0.5" y="@44" z="@0.5"/>
    <portal region="blue-portal" x="@-57.5" y="@2" z="@-57.5" yaw="@-45" filter="only-cyan"/>
    <portal region="lightblue-portal" x="@58.5" y="@2" z="@58.5" yaw="@135" filter="only-cyan"/>
    <portal region="red-portal" x="@58.5" y="@2" z="@-57.5" yaw="@45" filter="only-magenta"/>
    <portal region="pink-portal" x="@-57.5" y="@2" z="@58.5" yaw="@-135" filter="only-magenta"/>
</portals>
<renewables>
    <renewable region="bases" interval="5s">
        <renew-filter>
            <material>iron_block</material>
        </renew-filter>
        <replace-filter>
            <material>air</material>
        </replace-filter>
    </renewable>
    <renewable region="woolrooms" interval="1s" avoid-players="1">
        <renew-filter>
            <material>wool</material>
        </renew-filter>
        <replace-filter>
            <material>air</material>
        </replace-filter>
    </renewable>
</renewables>
<hunger>
    <depletion>off</depletion>
</hunger>
<damage>
    <deny>
        <all>
            <any>
                <cause>player</cause>
                <cause>projectile</cause>
            </any>            
            <region id="center"/>
        </all>
    </deny>
</damage>
<crafting>
    <disable>boat</disable>
</crafting>
<block-drops>
    <rule>
        <filter>
            <material>clay</material>
        </filter>
        <drops>
            <item material="clay"/>
        </drops>
    </rule>
</block-drops>
<broadcasts>
    <alert after="1s" filter="only-cyan">`6You joined the `3Cyan team`6! Let's go to the `9Blue side `6or the `bLight blue side `6with the `3Cyan-portal`6!</alert>
    <alert after="1s" filter="only-magenta">`6You joined the `dMagenta team`6! Let's go to the `4Red side `6or the `cPink side `6with the `dMagenta-portal`6!</alert>
</broadcasts>
</map>