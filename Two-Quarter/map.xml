        <rectangle min="105,-94" max="107,-93"/>
    
        <rectangle min="93,-97" max="94,-92"/>
        <rectangle min="94,-93" max="98,-92"/>
        <rectangle min="94,-95" max="95,-93"/>
        <rectangle min="95,-94" max="96,-93"/>
    
        <block>96,3,-104</block>
