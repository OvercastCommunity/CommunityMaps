<?xml version="1.0" encoding="utf-8"?>
<map proto="1.4.0">
<name>Two-Quarter</name>
<version>1.4</version>
<objective>Capture wools located on neighboring enemy sides!</objective>
<authors>
    <author uuid="5ebc601c-82b6-4cd2-88cf-1b5c552b8ace"/><!--Pe241-->
</authors>
<teams>
    <team id="blue" color="blue" max="12">Blue</team>
    <team id="red" color="dark_red" max="12">Red</team>
    <team id="orange" color="gold" max="12">Orange</team>
    <team id="purple" color="dark_purple" max="12">Purple</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item unbreakable="true" slot="0" material="iron sword"/>
        <item unbreakable="true" slot="1" enchantment="arrow infinite" material="bow"/>
        <item unbreakable="true" slot="2" material="diamond pickaxe"/>
        <item unbreakable="true" slot="3" material="iron axe"/>
        <item slot="4" material="golden apple"/>
        <item slot="6" amount="32" material="log"/>
        <item slot="9" material="arrow"/>
        <potion amplifier="50" duration="3">damage resistance</potion>
        <potion amplifier="50" duration="3">regeneration</potion>
    </kit>
    <kit id="blue-kit" parents="spawn-kit">
        <helmet color="0000aa" unbreakable="true" material="leather helmet"/>
        <chestplate color="0000aa" unbreakable="true" material="leather chestplate"/>
        <leggings color="0000aa" unbreakable="true" material="leather leggings"/>
        <boots color="0000aa" unbreakable="true" material="leather boots"/>
        <item slot="5" amount="32" damage="11" material="stained glass"/>
    </kit>
    <kit id="red-kit" parents="spawn-kit">
        <helmet color="aa0000" unbreakable="true" material="leather helmet"/>
        <chestplate color="aa0000" unbreakable="true" material="leather chestplate"/>
        <leggings color="aa0000" unbreakable="true" material="leather leggings"/>
        <boots color="aa0000" unbreakable="true" material="leather boots"/>
        <item slot="5" amount="32" damage="14" material="stained glass"/>
    </kit>
    <kit id="purple-kit" parents="spawn-kit">
        <helmet color="aa00aa" unbreakable="true" material="leather helmet"/>
        <chestplate color="aa00aa" unbreakable="true" material="leather chestplate"/>
        <leggings color="aa00aa" unbreakable="true" material="leather leggings"/>
        <boots color="aa00aa" unbreakable="true" material="leather boots"/>
        <item slot="5" amount="32" damage="10" material="stained glass"/>
    </kit>
    <kit id="orange-kit" parents="spawn-kit">
        <helmet color="ffaa00" unbreakable="true" material="leather helmet"/>
        <chestplate color="ffaa00" unbreakable="true" material="leather chestplate"/>
        <leggings color="ffaa00" unbreakable="true" material="leather leggings"/>
        <boots color="ffaa00" unbreakable="true" material="leather boots"/>
        <item slot="5" amount="32" damage="1" material="stained glass"/>
    </kit>
</kits>
<kill-rewards>
    <kill-reward>
        <item>golden apple</item>
    </kill-reward>
</kill-rewards>
<wools>
    <wool team="blue" color="red" location="100.5,18.5,-99.5">
        <monument>
            <block>-96,3,-104</block>
        </monument>
    </wool>
    <wool team="blue" color="orange" location="-99.5,18.5,100.5">
        <monument>
            <block>-104,3,-96</block>
        </monument>
    </wool>
    <wool team="red" color="blue" location="-99.5,18.5,-99.5">
        <monument>
            <block>96,3,-104</block>
        </monument>
    </wool>
    <wool team="red" color="purple" location="100.5,18.5,100.5">
        <monument>
            <block>104,3,-96</block>
        </monument>
    </wool>
    <wool team="purple" color="red" location="100.5,18.5,-99.5">
        <monument>
            <block>104,3,96</block>
        </monument>
    </wool>
    <wool team="purple" color="orange" location="-99.5,18.5,100.5">
        <monument>
            <block>96,3,104</block>
        </monument>
    </wool>
    <wool team="orange" color="blue" location="-99.5,18.5,-99.5">
        <monument>
            <block>-104,3,96</block>
        </monument>
    </wool>
    <wool team="orange" color="purple" location="100.5,18.5,100.5">
        <monument>
            <block>-96,3,104</block>
        </monument>
    </wool>
</wools>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather boots</item>
    <item>leather leggings</item>
    <item>string</item>
    <item>torch</item>
</itemremove>
<itemkeep>
    <item>iron sword</item>
    <item>bow</item>
    <item>diamond pickaxe</item>
    <item>iron axe</item>
    <item>iron helmet</item>
    <item>iron chestplate</item>
    <item>iron leggings</item>
    <item>iron boots</item>
    <item>golden apple</item>
    <item>log</item>
    <item>wood</item>
    <item>stained glass</item>
    <item>arrow</item>
</itemkeep>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>arrow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<filters>
    <team id="only-blue">blue</team>
    <team id="only-red">red</team>
    <team id="only-purple">purple</team>
    <team id="only-orange">orange</team>
    <deny id="no-blue">
        <team>blue</team>
    </deny>
    <deny id="no-red">
        <team>red</team>
    </deny>
    <deny id="no-orange">
        <team>orange</team>
    </deny>
    <deny id="no-purple">
        <team>purple</team>
    </deny>
    <deny id="deny-void">
        <void/>
    </deny>
    <deny id="allow-iron">
        <not><material>iron block</material></not>        
    </deny>
    <deny id="allow-woolroom-blue">
        <any>
            <team>blue</team>
            <all>
                <not><team>blue</team></not>
                <not>
                    <any>
                        <material>wool</material>
                        <material>web</material>
                    </any>
                </not>
            </all>
        </any>
    </deny>
    <deny id="allow-woolroom-red">
        <any>
            <team>red</team>
            <all>
                <not><team>red</team></not>
                <not>
                    <any>
                        <material>wool</material>
                        <material>web</material>
                    </any>
                </not>
            </all>
        </any>
    </deny>
    <deny id="allow-woolroom-orange">
        <any>
            <team>orange</team>
            <all>
                <not><team>orange</team></not>
                <not>
                    <any>
                        <material>wool</material>
                        <material>web</material>
                    </any>
                </not>
            </all>
        </any>
    </deny>
    <deny id="allow-woolroom-purple">
        <any>
            <team>purple</team>
            <all>
                <not><team>purple</team></not>
                <not>
                    <any>
                        <material>wool</material>
                        <material>web</material>
                    </any>
                </not>
            </all>
        </any>
    </deny>
    <deny id="deny-player">
        <cause>player</cause>
    </deny>
</filters>
<regions>
    <union id="purple-exception">
        <rectangle min="93,93" max="53,49"/>
        <rectangle min="93,93" max="94,98"/>
        <rectangle min="94,94" max="96,95"/>
        <rectangle min="94,94" max="95,96"/>
        
        <rectangle min="105,106" max="107,107"/>
        <rectangle min="106,105" max="107,107"/>
        <rectangle min="103,107" max="108,108"/>
        <rectangle min="107,103" max="108,108"/>
    
        <rectangle min="105,94" max="107,95"/>
        <rectangle min="106,95" max="107,96"/>
        <rectangle min="103,93" max="108,94"/>
        <rectangle min="107,94" max="108,97"/>
    
        <rectangle min="94,105" max="95,107"/>
        <rectangle min="95,106" max="96,107"/>
        <rectangle min="93,103" max="94,108"/>
        <rectangle min="94,107" max="98,108"/>
    
        <block>104,3,96</block>
        <block>96,3,104</block>
    </union>
    <union id="blue-exception">
        <rectangle min="-107,-97" max="-106,-92"/>
        <rectangle min="-106,-93" max="-102,-92"/>
        <rectangle min="-106,-95" max="-105,-93"/>
        <rectangle min="-105,-94" max="-104,-93"/>
    
        <rectangle min="-93,-97" max="-92,-92"/>
        <rectangle min="-97,-93" max="-92,-92"/>
        <rectangle min="-95,-94" max="-93,-93"/>
        <rectangle min="-94,-95" max="-93,-94"/>
        
        <rectangle min="-107,-107" max="-102,-106"/>
        <rectangle min="-107,-107" max="-106,-102"/>
        <rectangle min="-106,-106" max="-104,-105"/>
        <rectangle min="-106,-106" max="-105,-104"/>
    
        <rectangle min="-97,-107" max="-92,-106"/>
        <rectangle min="-93,-107" max="-92,-107"/>
        <rectangle min="-95,-106" max="-93,-105"/>
        <rectangle min="-94,-105" max="-93,-104"/>
    
        <block>-96,3,-104</block>
        <block>-104,3,-96</block>
    </union>
    <union id="red-exception">
        <rectangle min="93,-107" max="98,-106"/>
        <rectangle min="93,-107" max="94,-102"/>
        <rectangle min="94,-106" max="96,-105"/>
        <rectangle min="94,-106" max="95,-104"/>
            
