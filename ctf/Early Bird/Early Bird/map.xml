<map proto="1.5.0">
<name>Early Bird</name>
<version>1.0.0</version>
<phase>staging</phase>
<created>2025-12-14</created>
<objective>Capture the most flags in 8 minutes!</objective>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="1af9f952-e33e-42a5-8e1e-296757747fdc"/> <!-- Alphaboy98yt -->
</authors>
<contributors>
    <contributor uuid="63ddf58a-ba2c-4d39-8481-6358bb46c63a" contribution="xml"/> <!-- samschaap -->
</contributors>
<teams>
    <team id="blue-team" color="blue" max="15">Blue</team>
    <team id="red-team" color="dark red" max="15">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="2" unbreakable="true" material="shears"/>
        <item slot="3" amount="16" material="leaves"/>
        <item slot="4" amount="8" material="vine"/>
        <item slot="6" amount="20" material="arrow"/>
        <item slot="8" material="golden apple"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" enchantment="protection projectile:2" material="chainmail leggings"/>
        <boots unbreakable="true" material="iron boots"/>
        <effect duration="3s" amplifier="255">damage resistance</effect>
    </kit>
    <kit id="flag-pickup" force="true">
        <effect>slowness</effect>
    </kit>
    <kit id="flag-drop" force="true">
        <effect duration="0">slowness</effect>
    </kit>
</kits>
<spawns>
    <default>
        <region yaw="0">
            <cuboid min="124,134,-121" max="136,134,-119"/>
        </region>
    </default>
    <spawn team="blue-team" kit="spawn-kit">
        <region yaw="90">
            <point>199.5,86,-21.5</point>
        </region>
    </spawn>
    <spawn team="red-team" kit="spawn-kit">
        <region yaw="-90">
            <point>55.5,86,-21.5</point>
        </region>
    </spawn>
</spawns>
<filters>
    <any id="breakable-blocks">
        <material>leaves</material>
        <material>vine</material>
    </any>
</filters>
<regions>
    <negative id="void-area">
        <rectangle id="playable-area" min="51,-74" max="204,31"/>
    </negative>
    <union id="spawns">
        <rectangle id="blue-spawn" min="192,-26" max="204,-17"/>
        <rectangle id="red-spawn" min="63,-17" max="51,-26"/>
    </union>
    <union id="nets">
        <cylinder id="blue-net" base="174.5,95,-56.5" radius="3" height="5"/>
        <cylinder id="red-net" base="80.5,95,13.5" radius="3" height="5"/>
    </union>
    <apply block="never" region="spawns" message="You may not obstruct the spawns!"/>
    <apply block="never" region="nets" message="You may not obstruct the area around the flag!"/>
    <apply block-break="breakable-blocks" message="You may only break leaves and vines!"/>
    <apply block="never" region="void-area" message="You may not build outside the map!"/>
</regions>
<flags>
    <flag id="blue-flag" name="Blue Flag" owner="blue-team" pickup-kit="flag-pickup" drop-kit="flag-drop">
        <post recover-time="7s" respawn-time="15s" pickup-filter="red-team">174.5,96,-56.5</post>
    </flag>
    <flag id="red-flag" name="Red Flag" owner="red-team" pickup-kit="flag-pickup" drop-kit="flag-drop">
        <post recover-time="7s" respawn-time="15s" pickup-filter="blue-team">80.5,96,13.5</post>
    </flag>
    <flags points="1">
        <net region="blue-net" flag="red-flag"/>
        <net region="red-net" flag="blue-flag"/>
    </flags>
</flags>
<time>8m</time>
<score>
    <limit>5</limit>
</score>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
    <tool>shears</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>iron boots</item>
    <item>sapling</item>
</itemremove>
<itemkeep>
    <item>golden apple</item>
    <item>arrow</item>
    <item>leaves</item>
    <item>vine</item>
</itemkeep>
<kill-rewards>
    <kill-reward>
        <item amount="10" material="arrow"/>
        <item amount="16" material="leaves"/>
        <item amount="4" material="vine"/>
    </kill-reward>
</kill-rewards>
<respawn delay="3s"/>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
