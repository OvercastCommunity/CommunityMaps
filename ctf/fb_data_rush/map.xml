<map proto="1.5.0">
<name>FB: Data-Rush</name>
<!--<slug>tvt_data_rush</slug>-->
<variant id="comp" override="true">FB: Super-Rush</variant>
<variant id="modern" world="modern" override="true" min-server-version="1.21.9">FB: Data-Rush ⟨CMP Edition⟩</variant>
<version>2.0.4</version>
<phase>development</phase>
<created>2025-11-15</created>
<constants>
    <constant id="team-size">12</constant>
    <constant id="build-limit">20</constant>
    <constant id="default-leaves-classic-damage">3</constant>
    <constant id="default-leaves-modern">jungle leaves</constant>
    <constant id="default-planks-classic">3</constant>
    <constant id="default-planks-modern">mangrove planks</constant>
    <constant id="deny-use">custom-deny-use</constant>
    <constant id="map-center">0.5,0.0,0.5</constant>
    <constant id="central-island-chest-region">outer-platforms</constant>
    <constant id="central-island-chest-refill-timer">10s</constant> 
    <if variant="comp">
        <constant id="kill-teams">1</constant>
    </if>
    <constant id="obs-yaw">90</constant>
    <constant id="obs-pitch">0</constant>
    <constant id="team-one-yaw">-90</constant>
    <constant id="team-one-pitch">0</constant>
    <constant id="team-one-flag-post-1-yaw">0</constant>
    <constant id="team-one-flag-post-2-yaw">0</constant>
    <constant id="team-two-yaw">0</constant>
    <constant id="team-two-pitch">0</constant>
    <constant id="team-two-flag-post-1-yaw">90</constant>
    <constant id="team-two-flag-post-2-yaw">90</constant>
    <constant id="team-three-yaw">90</constant>
    <constant id="team-three-pitch">0</constant>
    <constant id="team-three-flag-post-1-yaw">180</constant>
    <constant id="team-three-flag-post-2-yaw">180</constant>
    <constant id="team-four-yaw">180</constant>
    <constant id="team-four-pitch">0</constant>
    <constant id="team-four-flag-post-1-yaw">-90</constant>
    <constant id="team-four-flag-post-2-yaw">-90</constant>
    <!-- Disable legacy PvP -->
    <constant id="legacy-pvp">false</constant>
</constants>
<include id="flag-battles"/>
<authors>
    <author uuid="1aad55f7-2dea-4f22-85ca-ad9de3a78609" contribution="Map Author"/> <!-- Technodono -->
</authors>
<contributors>
    <contributor uuid="9602cd49-4779-4965-846f-67d49bce48a8" contribution="Map Layout"/> <!-- Abuelos -->
</contributors>
<filters>
    <deny id="custom-deny-use">
        <any>
            <material>lever</material>
            <material>note block</material>
            <material>hopper</material>
            <material>furnace</material>
            <material>anvil</material>
        </any>
    </deny>
</filters>
<regions>
    <apply block="never" region="flag-post-deny-zones" message="You may not modify the flag posts!"/>
    <apply block="never" region="spawner-deny-zone" message="You may not modify the spawner region!"/>

    <point id="team-one-spawnpoint">-53.5,8,-53.5</point>
    <point id="team-two-spawnpoint">54.5,8,-53.5</point>
    <point id="team-three-spawnpoint">54.5,8,54.5</point>
    <point id="team-four-spawnpoint">-53.5,8,54.5</point>
    <cuboid id="obs-spawnpoint" min="4,58,4" max="-1,58,-3"/>

    <cuboid id="team-one-portal" min="-53,8,-37" size="1,6,-5"/>
    <cuboid id="team-two-portal" min="38,8,-53" size="5,6,1"/>
    <cuboid id="team-three-portal"  min="54,8,38" size="-1,6,5"/>
    <cuboid id="team-four-portal" min="-37,8,54" size="-5,6,-1"/>

    <cuboid id="team-one-portal-indicator" min="-54,33,-37" size="1,-9,-5"/>
    <cuboid id="team-two-portal-indicator" min="38,33,-54" size="5,-9,1"/>
    <cuboid id="team-three-portal-indicator" min="55,33,38" size="-1,-9,5"/>
    <cuboid id="team-four-portal-indicator" min="-37,33,55" size="-5,-9,-1"/>

    <cuboid id="mid-room" min="-15,0,15" max="16,20,-15"/>
    <cuboid id="spawner-deny-zone" min="-1,9,-1" size="3,2,3"/>
    <union id="flag-post-deny-zones">
        <cuboid id="team-one-posts" min="-45,8,-48" size="5,5,3"/>
        <cuboid id="team-four-posts" min="-48,8,46" size="3,5,-5"/>
        <mirror normal="1,0,0" origin="${map-center}">
            <mirror normal="0,0,1" origin="${map-center}" region="team-one-posts"/> <!--team-three post-->
        </mirror>
        <mirror normal="1,0,0" origin="${map-center}">
            <mirror normal="0,0,1" origin="${map-center}" region="team-four-posts"/> <!--team-two post-->
        </mirror>
    </union>

    <union id="outer-platforms">
        <block id="north-platform-chest">0,8,-55</block>
        <mirror normal="0,0,1" origin="${map-center}" region="north-platform-chest"/>
        <mirror normal="-1,0,1" origin="${map-center}" region="north-platform-chest"/>
        <mirror normal="1,0,1" origin="${map-center}" region="north-platform-chest"/>
    </union>

    <block id="team-one-flag-1-post">-42,8,-47</block>
    <block id="team-one-flag-2-post">-44, 8, -47</block>
    <block id="team-two-flag-1-post">47, 8, -44</block>
    <block id="team-two-flag-2-post">47, 8, -42</block>
    <block id="team-three-flag-1-post">44, 8, 47</block>
    <block id="team-three-flag-2-post">42, 8, 47</block>
    <block id="team-four-flag-1-post">-47, 8, 44</block>
    <block id="team-four-flag-2-post">-47, 8, 42</block>

    <complement id="team-one-spawnroom">
        <cuboid min="-56,0,-57" size="24,19,7"/>
        <cuboid min="-39,8,-50" size="7,5,-7"/>
    </complement>
    <complement id="team-two-spawnroom">
        <cuboid min="58,0,-56" size="-7,19,24"/>
        <cuboid min="51,8,-39" size="7,5,7"/>
    </complement>
    <complement id="team-three-spawnroom">
        <cuboid min="57,0,58" size="-24,19,-7"/>
        <cuboid min="40,8,51" size="-7,5,7"/>
    </complement>
    <complement id="team-four-spawnroom">
        <cuboid min="-57,0,57" size="7,19,-24"/>
        <cuboid min="-50,8,40" size="-7,5,-7"/>
    </complement>

    <cuboid id="team-one-spawnroom-door" min="-33,14,-51" size="-5,5,1"/>
    <cuboid id="team-two-spawnroom-door" min="52,14,-33" size="-1,5,-5"/>
    <cuboid id="team-three-spawnroom-door" min="34,14,52" size="5,5,-1"/>
    <cuboid id="team-four-spawnroom-door" min="-51,14,34" size="1,5,5"/>
    <cuboid id="map-buildable-area" min="-58,0,-58" max="59,59,59"/>
</regions>
<spawners>
    <spawner spawn-region="spawner-deny-zone" player-region="mid-room" max-entities="1" delay="15s">
        <item amount="1" material="golden apple"/>
    </spawner>
    <spawner spawn-region="spawner-deny-zone" player-region="mid-room" max-entities="1" delay="5s">
        <potion damage="11">
            <effect duration="35s" amplifier="1">speed</effect>
            <effect duration="35s" amplifier="2">jump_boost</effect>
        </potion>
    </spawner>
</spawners>
<world>
    <timelock>on</timelock>
    <timeset>1200</timeset>
</world>
</map>
