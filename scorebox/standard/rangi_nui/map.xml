        </spawn-region>
        <player-region>
            <region id="red-top-region"/>
        </player-region>
        <item material="double_plant" damage="0" name="`e`lCoin" lore="`7`oJump into the middle pit with coins to earn points."/>
    </spawner>
    <!-- Red Mid Spawner -->
