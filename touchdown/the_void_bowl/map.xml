    <cuboid id="flag-indicator-lower" min="45,10,28" max="48,12,31"/>
    <cuboid id="flag-indicator-upper-inner" min="46,13,29" max="47,18,30"/>
    <cuboid id="flag-indicator-lower-inner" min="46,10,29" max="47,12,30"/>
</regions>
