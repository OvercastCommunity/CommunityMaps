            </replace-item>
            <set var="power_number" value="100"/>
        </action>
    </trigger>
        <!-- Double-Jump Potion Logic-->
    <action id="give-doublejump" scope="player">
        <set var="doublejump_state" value="1"/>
    </action>
    <action id="remove-doublejump" scope="player">
