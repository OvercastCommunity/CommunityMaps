    <kit id="power-0" deduct-items="false">
        <item material="cactus" name="`eTall Cactus" amount="3" consumable="tall-cacti-item" enchantment="fortune"/>
        <action>
            <message text="You unboxed a `eTall Cactus"/>
        </action>
    </kit>
    <kit id="power-1" deduct-items="false">
