            </payment>
            <success-action>
                <paste-structure structure="tall-cacti" x="player.place_x+1" y="10" z="player.place_z+1" update="false"/>
            </success-action>
        </take-payment>
    </action>
    <action id="paste-taller-cacti" scope="player" filter="looking_at_block">
