    </action>
    <action id="give-kit-9" scope="player">
        <kit id="lvl-9-kit"/>
    </action>
    <!-- #endregion kit-actions -->
    <action id="announce-auto-tp" scope="player">
        <message text="`e⚠ `6`lIt looks like you're struggling to complete the level."/>
        <message text="You've been moved to the next level to get a chance to complete the map!"/>
    </action>
    <action id="increase-tp-level" scope="match">
        <set var="tp_level" value="tp_level + 1"/>
    </action>
    <action id="randomize-tp-point" scope="player">
        <set var="tp_rnd_point" value="floor(random() * 9)"/>
    </action>
    <!-- #endregion actions -->
    <!-- #region triggers -->
    <!-- #region triggers-kits -->
    <trigger filter="lvl-2" scope="player" action="give-kit-2"/>
    <trigger filter="lvl-3" scope="player" action="give-kit-3"/>
    <trigger filter="lvl-4" scope="player" action="give-kit-4"/>
    <trigger filter="lvl-5" scope="player" action="give-kit-5"/>
    <trigger filter="lvl-6" scope="player" action="give-kit-6"/>
    <trigger filter="lvl-7" scope="player" action="give-kit-7"/>
    <trigger filter="lvl-8" scope="player" action="give-kit-8"/>
    <trigger filter="lvl-9" scope="player" action="give-kit-9"/>
    <!-- #endregion triggers-kits -->
    <!-- #region triggers-auto-tp -->
    <trigger filter="running" scope="player" action="randomize-tp-point"/>
    <trigger filter="on-teleport" scope="player" action="randomize-tp-point"/>
    <trigger filter="on-teleport" scope="player" action="announce-auto-tp"/>
    <trigger filter="on-tp-lvl-1" scope="match" action="increase-tp-level"/>
    <trigger filter="on-tp-lvl-2" scope="match" action="increase-tp-level"/>
    <trigger filter="on-tp-lvl-3" scope="match" action="increase-tp-level"/>
    <trigger filter="on-tp-lvl-4" scope="match" action="increase-tp-level"/>
    <trigger filter="on-tp-lvl-5" scope="match" action="increase-tp-level"/>
    <trigger filter="on-tp-lvl-6" scope="match" action="increase-tp-level"/>
    <trigger filter="on-tp-lvl-7" scope="match" action="increase-tp-level"/>
    <trigger filter="on-tp-lvl-8" scope="match" action="increase-tp-level"/>
    <!-- #endregion triggers-auto-tp -->
    <!-- #endregion triggers -->
</actions>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>entity attack</damage>
    <damage>fall</damage>
    <damage>lava</damage>
</disabledamage>
