<filters>
    <!-- #region scores -->
    <score id="lvl-1">0</score>
    <score id="lvl-2">1</score>
    <score id="lvl-3">2</score>
    <score id="lvl-4">3</score>
    <score id="lvl-5">4</score>
    <score id="lvl-6">5</score>
    <score id="lvl-7">6</score>
    <score id="lvl-8">7</score>
    <score id="lvl-9">8</score>
    <score id="lvl-10">9</score>
    <!-- #endregion scores -->
    <!-- #region tp-countdowns -->
    <after duration="${TP_COUNTDOWN}" id="on-tp-lvl-1" filter="all(running, tp_level=0)"/>
    <after duration="${TP_COUNTDOWN}" id="on-tp-lvl-2" filter="all(running, tp_level=1)"/>
    <after duration="${TP_COUNTDOWN}" id="on-tp-lvl-3" filter="all(running, tp_level=2)"/>
    <after duration="${TP_COUNTDOWN}" id="on-tp-lvl-4" filter="all(running, tp_level=3)"/>
    <after duration="${TP_COUNTDOWN}" id="on-tp-lvl-5" filter="all(running, tp_level=4)"/>
    <after duration="${TP_COUNTDOWN}" id="on-tp-lvl-6" filter="all(running, tp_level=5)"/>
    <after duration="${TP_COUNTDOWN}" id="on-tp-lvl-7" filter="all(running, tp_level=6)"/>
    <after duration="${TP_COUNTDOWN}" id="on-tp-lvl-8" filter="all(running, tp_level=7)"/>
    <countdown duration="${TP_COUNTDOWN_ANNOUNCEMENT_LENGTH}" message="Teleporting to the next level in {0}">
        <any>
            <after duration="${TP_COUNTDOWN_ANNOUNCEMENT}" filter="all(running, tp_level=0, lvl-1)" />
            <after duration="${TP_COUNTDOWN_ANNOUNCEMENT}" filter="all(running, tp_level=1, lvl-2)" />
            <after duration="${TP_COUNTDOWN_ANNOUNCEMENT}" filter="all(running, tp_level=2, lvl-3)" />
            <after duration="${TP_COUNTDOWN_ANNOUNCEMENT}" filter="all(running, tp_level=3, lvl-4)" />
            <after duration="${TP_COUNTDOWN_ANNOUNCEMENT}" filter="all(running, tp_level=4, lvl-5)" />
            <after duration="${TP_COUNTDOWN_ANNOUNCEMENT}" filter="all(running, tp_level=5, lvl-6)" />
            <after duration="${TP_COUNTDOWN_ANNOUNCEMENT}" filter="all(running, tp_level=6, lvl-7)" />
            <after duration="${TP_COUNTDOWN_ANNOUNCEMENT}" filter="all(running, tp_level=7, lvl-8)" />
        </any>
    </countdown>
    <any id="on-teleport">
        <filter id="all(alive, on-tp-lvl-1, lvl-1)"/>
        <filter id="all(alive, on-tp-lvl-2, lvl-2)"/>
        <filter id="all(alive, on-tp-lvl-3, lvl-3)"/>
        <filter id="all(alive, on-tp-lvl-4, lvl-4)"/>
        <filter id="all(alive, on-tp-lvl-5, lvl-5)"/>
        <filter id="all(alive, on-tp-lvl-6, lvl-6)"/>
        <filter id="all(alive, on-tp-lvl-7, lvl-7)"/>
        <filter id="all(alive, on-tp-lvl-8, lvl-8)"/>
    </any>
    <!-- #endregion tp-countdowns -->
    <all id="allow-using-boats">
        <holding>
            <item material="boat"/>
        </holding>
        <filter id="lvl-3"/>
        <!-- Disallow opening trapdoors -->
        <not>
            <any>
                <material>wood button</material>
                <material>trap door</material>
            </any>
        </not>
    </all>
    <match-running id="running"/>
    <alive id="alive"/>
</filters>
<actions>
    <!-- #region actions -->
    <!-- #region kit-actions -->
    <action id="give-kit-2" scope="player">
        <kit id="lvl-2-kit"/>
    </action>
    <action id="give-kit-3" scope="player">
        <kit id="lvl-3-kit"/>
    </action>
    <action id="give-kit-4" scope="player">
        <kit id="lvl-4-kit"/>
    </action>
    <action id="give-kit-5" scope="player">
