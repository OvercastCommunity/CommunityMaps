        </monument>
    </wool>
</wools>
<blitz>
    <lives>1</lives>
</blitz>
<spawners>
    <spawner spawn-region="wool-spawn" player-region="wool-player" delay="0.5s">
        <item amount="1" material="wool" damage="15"/>
    </spawner>
</spawners>
<hunger>
    <depletion>off</depletion>
</hunger>
<itemremove>
    <item>diamond sword</item>
    <item>leather chestplate</item>
