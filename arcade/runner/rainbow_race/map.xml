    <kit id="speed-2-kit" force="true">
        <walk-speed>1.4</walk-speed>
    </kit>
    <kit id="speed-8-kit" force="true">
        <walk-speed>2.6</walk-speed>
    </kit>
    <kit id="void-death-kit" force="true">
        <health>1</health>
    </kit>
</kits>
