<?xml version="1.0"?>
<map proto="1.4.0">
<name>Harbor CTW</name>
<version>1.0.0</version>
<objective>Capture the wool!</objective>
<gamemode>ctw</gamemode>
<phase>development</phase>
<created>2021-07-30</created>
<authors>
    <author uuid="fe3608b7-d105-4029-8800-34b3147065b6" contribution="executive producer"/> <!-- rockymine -->
    <author uuid="e2d2c2c6-cea9-4510-9ab2-a091b5605b30" contribution="head of realization, post production, xml"/> <!-- Ruediger_LP  -->
    <author uuid="cc01304f-907b-4c80-b9c2-8f61872969d1" contribution="head of interior design"/> <!-- _Just -->
    <author uuid="be80632d-f050-4e4e-8e73-659d977c90d7" contribution="commercial space interior design"/> <!-- Artdamensio  -->
</authors>
<contributors>
    <contributor uuid="f876b863-1ccd-4ae2-acad-8a09e2209e81" contribution="original idea"/> <!-- _DISTRACTION_ -->
</contributors>
<teams>
    <team id="blue-team" color="blue" max="50">Blue</team>
    <team id="red-team" color="red" max="50">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" material="iron sword" unbreakable="true"/>
        <item slot="1" material="bow" unbreakable="true">
            <enchantment>arrow infinite</enchantment>
        </item>
        <item slot="2" material="diamond pickaxe" unbreakable="true">
            <enchantment>dig speed</enchantment>
        </item>
        <item slot="3" material="iron axe" unbreakable="true">
            <enchantment level="2">dig speed</enchantment>
        </item>
        <item slot="4" material="wood" amount="64"/>
        <item slot="6" material="vine" amount="24"/>
        <item slot="8" material="golden apple"/>
        <item slot="28" material="arrow"/>
        <item slot="30" material="shears" unbreakable="true"/>
        <item slot="29" material="iron spade" unbreakable="true"/>
        <helmet material="iron helmet" unbreakable="true"/>
        <chestplate material="chainmail chestplate" unbreakable="true"/>
        <effect duration="oo" amplifier="100">damage resistance</effect>
        <effect duration="oo" amplifier="2">speed</effect>
    </kit>
    <kit force="true" id="reset-effects-kit">
        <effect duration="0" amplifier="0">damage resistance</effect>
        <effect duration="0" amplifier="0">speed</effect>
    </kit>
    <kit id="blue-kit" parent="spawn-kit">
        <item slot="5" material="stained clay" damage="11" amount="48"/>
        <leggings material="leather leggings" color="2471A3" unbreakable="true"/>
        <boots material="leather boots" color="2471A3" unbreakable="true"/>
    </kit>
    <kit id="red-kit" parent="spawn-kit">
        <item slot="5" material="stained clay" damage="14" amount="48"/>
        <leggings material="leather leggings" color="CB4335" unbreakable="true"/>
        <boots material="leather boots" color="CB4335" unbreakable="true"/>
    </kit>
</kits>
<spawns>
    <spawn team="blue-team" kit="blue-kit" region="blue-spawn-point" yaw="-112.5"/>
    <spawn team="red-team" kit="red-kit" region="red-spawn-point" yaw="67.5"/>
    <default region="obs-spawn" yaw="-135" pitch="20"/>
</spawns>
<wools craftable="false">
    <wool team="blue-team" color="orange" monument="blue-team-orange-wool" location="134.5,10.5,-30.5"/>
    <wool team="blue-team" color="magenta" monument="blue-team-magenta-wool" location="150.5,21.5,122.5"/>
    <wool team="red-team" color="yellow" monument="red-team-yellow-wool" location="-134.5,10.5,30.5"/>
    <wool team="red-team" color="lime" monument="red-team-lime-wool" location="-150.5,21.5,-122.5"/>
</wools>
<filters>
    <team id="only-blue">blue-team</team>
    <team id="only-red">red-team</team>
    <!-- place -->
    <all id="reds-woolrooms-place-filter">
        <team>blue-team</team>
        <filter id="woolrooms-place-filter"/>
    </all>
    <all id="blues-woolrooms-place-filter">
        <team>red-team</team>
        <filter id="woolrooms-place-filter"/>
    </all>
    <any id="woolrooms-place-filter">
        <material>wood</material>
        <material>stained clay</material>
        <material>vine</material>
        <material>workbench</material>
    </any>
    <!-- break -->
    <all id="reds-woolrooms-break-filter">
        <team>blue-team</team>
        <filter id="woolrooms-break-filter"/>
    </all>
    <all id="blues-woolrooms-break-filter">
        <team>red-team</team>
        <filter id="woolrooms-break-filter"/>
    </all>
    <any id="woolrooms-break-filter">
        <material>wool:1</material>
        <material>wool:2</material>
        <material>wool:4</material>
        <material>wool:5</material>
        <material>web</material>
        <material>wood</material>
        <material>stained clay</material>
        <material>vine</material>
        <material>iron block</material>
        <material>workbench</material>
    </any>
    <material id="only-iron">iron block</material>
    <deny id="deny-use">
        <any>
            <material>dropper</material>
            <material>anvil</material>
        </any>
    </deny>
    <material id="only-wool-1">wool:1</material>
    <material id="only-wool-2">wool:2</material>
    <material id="only-wool-4">wool:4</material>
    <material id="only-wool-5">wool:5</material>
    <all id="blue-and-stained-clay">
        <team>blue-team</team>
        <material>stained clay</material>
    </all>
    <all id="red-and-stained-clay">
        <team>red-team</team>
        <material>stained clay</material>
    </all>
    <any id="not-void-or-void-and-leaves-or-logs">
        <not id="not-void">
            <void/>
        </not>
        <all>
            <void/>
            <any>
                <material>leaves</material>
                <material>log</material>
            </any>
        </all>
    </any>
</filters>
<regions>
    <cylinder id="blue-spawn-point" base="-183.5,15.2,-72.5" radius="2" height="0"/>
    <cylinder id="red-spawn-point" base="183.5,15.2,72.5" radius="2" height="0"/>
    <cylinder id="obs-spawn" base="-66.75,54.2,66.75" radius="1" height="0"/>
    <block id="blue-team-orange-wool">-178,16,-81</block>
    <block id="blue-team-magenta-wool">-174,16,-81</block>
    <block id="red-team-yellow-wool">177,16,80</block>
    <block id="red-team-lime-wool">173,16,80</block>
    <negative id="not-spawns">
        <union id="spawns">
            <rectangle id="blue-spawn" min="-251,-93" max="-139,-37"/>
            <rectangle id="red-spawn" min="139,37" max="251,93"/>
        </union>
    </negative>
    <union id="woolrooms">
        <union id="blues-woolrooms"> <!-- blue defends -->
            <rectangle min="-145,17" max="-129,35"/> <!-- yellow woolroom -->
            <rectangle min="-155,-133" max="-137,-117"/> <!-- lime woolroom -->
        </union>
        <union id="reds-woolrooms"> <!-- red defends -->
            <rectangle min="129,-35" max="145,-17"/> <!-- orange woolroom -->
            <rectangle min="137,117" max="155,133"/> <!-- magenta woolroom -->
        </union>
    </union>
    <negative id="not-build-area">
        <union id="bluild-area">
            <rectangle min="-59,-87" max="59,87"/> <!-- mid -->
            <rectangle min="-97,-117" max="-53,-53"/> <!-- blue -->
            <rectangle min="-129,-37" max="-97,11"/>
            <rectangle min="-119,-69" max="-97,-53"/>
            <rectangle min="53,53" max="97,117"/> <!-- red -->
            <rectangle min="97,-11" max="129,37"/>
            <rectangle min="97,53" max="119,69"/>
        </union>
    </negative>
    <union id="mountains-block">
        <rectangle min="-145,27" max="-177,-37"/> <!-- blue -->
        <rectangle min="-138,-37" max="-145,-5"/>
        <rectangle min="177,37" max="145,-27"/> <!-- red -->
        <rectangle min="145,5" max="138,37"/>
    </union>
    <union id="mountains-enter">
        <rectangle min="-156,27" max="-177,-37"/> <!-- blue -->
        <rectangle min="177,37" max="156,-27"/> <!-- red -->
    </union>
    <union id="beacon-protection">
    <circle radius="0.5" center="134.5,-30.5"/>
    <circle radius="0.5" center="150.5,122.5"/>
    <circle radius="0.5" center="-134.5,30.5"/>
    <circle radius="0.5" center="-150.5,-122.5"/>
</union>
<union id="woolrooms-or-spawns">
    <region id="woolrooms"/>
    <region id="spawns"/>
</union>
    <!-- applicators -->
    <apply enter="only-blue" region="blue-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-red" region="red-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-red" region="blues-woolrooms" message="You may not enter your own wool room!"/>
    <apply enter="only-blue" region="reds-woolrooms" message="You may not enter your own wool room!"/>
    <apply enter="never" region="mountains-enter" message="You may not leave the palying field!"/>
    <apply block-place="never" block-break="only-iron" region="spawns" message="You may not edit spawn!"/>
    <apply block="never" region="beacon-protection" message="You may not edit the wool room!"/>
    <apply block-place="blues-woolrooms-place-filter" block-break="blues-woolrooms-break-filter" region="blues-woolrooms" message="You may not edit the wool room!"/>
    <apply block-place="reds-woolrooms-place-filter" block-break="reds-woolrooms-break-filter" region="reds-woolrooms" message="You may not edit the wool room!"/>
    <apply block="never" region="mountains-block" message="You may not edit the void!"/>
    <apply block-place="not-void" block-break="not-void-or-void-and-leaves-or-logs" region="not-build-area" message="You may not edit the void"/>
    <apply use="deny-use" region="everywhere"/>
    <apply kit="reset-effects-kit" region="not-spawns"/>
</regions>
<block-drops>
    <rule wrongtool="false" region="woolrooms-or-spawns" filter="only-iron">
        <drops>
            <item material="iron block"/>
        </drops>
        <replacement>iron block</replacement>
    </rule>
    <!-- stained clay -->
    <rule wrongtool="false" region="everywhere" filter="blue-and-stained-clay">
        <drops>
            <item damage="11" material="stained clay"/>
        </drops>
    </rule>
    <rule wrongtool="false" region="everywhere" filter="red-and-stained-clay">
        <drops>
            <item damage="14" material="stained clay"/>
        </drops>
    </rule>
    <!-- wools -->
    <rule wrongtool="false" region="woolrooms" filter="only-wool-1">
        <drops>
            <item damage="1" material="wool"/>
        </drops>
        <replacement>wool:1</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-2">
        <drops>
            <item damage="2" material="wool"/>
        </drops>
        <replacement>wool:2</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-4">
        <drops>
            <item damage="4" material="wool"/>
        </drops>
        <replacement>wool:4</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-5">
        <drops>
            <item damage="5" material="wool"/>
        </drops>
        <replacement>wool:5</replacement>
    </rule>
</block-drops>
<itemkeep>
    <item>iron sword</item>
    <item>bow</item>
    <item>diamond pickaxe</item>
    <item>iron axe</item>
    <item>shears</item>
    <item>arrow</item>
    <item>iron spade</item>
    <item>stained clay</item>
    <item>leaves</item>
    <item>vine</item>
    <item>golden apple</item>
</itemkeep>
<itemremove>
    <item>anvil</item>
    <item>dropper</item>
    <item>dirt</item>
    <item>stone</item>
    <item>cobblestone</item>
    <item>red rose</item>
    <item>carpet</item>
    <item>wool:15</item>
    <item>wool:7</item>
    <item>string</item>
    <item>smooth brick</item>
    <item>step</item>
    <item>long grass</item>
    <item>smooth stairs</item>
    <item>nether brick</item>
    <item>cobble wall</item>
    <item>wood step:3</item>
    <item>wood:1</item>
    <item>wood:3</item>
    <item>stone button</item>
    <item>seeds</item>
    <item>quartz stairs</item>
    <item>jungle wood stairs</item>
    <item>iron trapdoor</item>
    <item>sapling</item>
    <item>iron helmet</item>
    <item>chainmail chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
    <tool>arrow</tool>
    <tool>shears</tool>
    <tool>iron spade</tool>
</toolrepair>
<item-mods>
    <rule>
        <match>
            <material>iron helmet</material>
            <material>iron chestplate</material>
            <material>iron leggings</material>
            <material>iron boots</material>
            <material>iron sword</material>
            <material>iron pickaxe</material>
            <material>iron axe</material>
            <material>iron spade</material>
            <material>shears</material>
        </match>
        <modify unbreakable="true"/>
    </rule>
</item-mods>
<crafting>
    <!-- bulkcrafting -->
    <shaped override="false">
        <result material="iron helmet" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron chestplate" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>III</row>
            <row>III</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron leggings" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="bucket" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>.I.</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
</crafting>
<hunger>
    <depletion>off</depletion>
</hunger>
<gamerules>
    <radomTickSpeed>3</radomTickSpeed>
</gamerules>
<maxbuildheight>40</maxbuildheight>
<kill-rewards>
    <kill-reward>
        <item material="golden apple"/>
    </kill-reward>
    <kill-reward filter="only-blue">
        <item slot="5" material="stained clay" damage="11" amount="48"/>
    </kill-reward>
    <kill-reward filter="only-red">
        <item slot="5" material="stained clay" damage="14" amount="48"/>
    </kill-reward>
</kill-rewards>
</map>
