<!--Misc-->
<maxbuildheight>24</maxbuildheight>
<respawn delay="3s" blackout="true" auto="true"/>
<broadcasts>
    <alert after="15s" every="10m">Creating alliances with another team is allowed.</alert>
</broadcasts>
</map>
