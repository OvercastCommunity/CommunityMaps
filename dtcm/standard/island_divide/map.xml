<map proto="1.4.2">
<name>Island Divide</name>
<version>1.1.0</version>
<phase>development</phase>
<objective>Destroy the opponent's monuments at the opposite side of the island and enjoy the sun!</objective>
<gamemode>dtm</gamemode>
<authors>
    <author uuid="060ae7e1-43a0-42ad-8b5d-7240c5ec05af"/> <!-- EsotericSorcery -->
</authors>
<teams>
    <team id="blue-team" color="blue" max="20">Blue</team>
    <team id="green-team" color="green" max="20">Green</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" material="iron sword" unbreakable="true"/>
        <item slot="1" material="bow" unbreakable="true" enchantment="arrow infinite"/>
        <item slot="2" material="diamond pickaxe" unbreakable="true"/>
        <item slot="3" material="iron axe" unbreakable="true">
            <enchantment level="2">dig speed</enchantment>
        </item>
        <item slot="4" material="water bucket" amount="1"/>
        <item slot="5" material="wood" amount="64" damage="3"/>
        <item slot="7" material="vine" amount="32"/>
        <item slot="8" material="golden apple"/>
        <item slot="28" material="arrow"/>
        <item slot="30" material="iron spade" unbreakable="true"/>
        <item slot="34" material="torch" amount="16"/>
        <item slot="35" material="shears" unbreakable="true"/>
        <leggings material="chainmail leggings" unbreakable="true">
            <enchantment level="1">projectile_protection</enchantment>
        </leggings>
        <effect duration="3s" amplifier="255">damage resistance</effect>
    </kit>
    <kit id="blue-kit" parents="spawn-kit">
        <chestplate color="3AB3DA" unbreakable="true">leather chestplate</chestplate>
        <boots color="3AB3DA" unbreakable="true">leather boots</boots>
        <item slot="6" damage="3" amount="48">stained clay</item>
    </kit>
    <kit id="green-kit" parents="spawn-kit">
        <chestplate color="80C71F" unbreakable="true">leather chestplate</chestplate>
        <boots color="80C71F" unbreakable="true">leather boots</boots>
        <item slot="6" damage="5" amount="48">stained clay</item>
    </kit>
</kits>
<spawns>
    <default yaw="90">
        <region>
            <point>105.5,30,0.5</point>
        </region>
    </default>
    <spawn team="blue-team" kit="blue-kit">
        <region yaw="0">
            <point>69.5,10,-99.5</point>
        </region>
    </spawn>
    <spawn team="green-team" kit="green-kit">
        <region yaw="-180">
            <point>-68.5,10,100.5</point>
        </region>
    </spawn>
</spawns>
<destroyables completion="100%" required="true" sparks="true">
    <destroyable name="Hill Monument" id="blue-obsidian" materials="obsidian" owner="blue-team" modes="beacon-mode coal-mode">
        <region>
           <cuboid min="-22,22,-69" max="-21,25,-80"/>
        </region>
    </destroyable>
    <destroyable name="Hill Monument" id="green-obsidian" materials="obsidian" owner="green-team" modes="beacon-mode coal-mode">
        <region>
           <cuboid min="23,22,80" max="22,25,81"/>
        </region>
    </destroyable>
    <destroyable name="Beach Monument" id="blue-emerald" materials="emerald block" owner="blue-team" show-progress="true">
        <region>
           <cuboid min="33,16,-90" max="36,19,-93"/>
        </region>
    </destroyable>
    <destroyable name="Beach Monument" id="green-emerald" materials="emerald block" owner="green-team" show-progress="true">
        <region>
           <cuboid min="-32,16,91" max="-35,19,94"/>
        </region>
    </destroyable>
</destroyables>
<modes>
    <mode id="beacon-mode" after="15m" name="`bBEACON MONUMENT MODE `e(Hill Monument)" material="beacon"/>
    <mode id="coal-mode" after="30m" name="`0COAL MONUMENT MODE `e(Hill Monument)" material="coal block"/>
</modes>
<filters>
    <team id="only-blue">blue-team</team>
    <team id="only-green">green-team</team>
    <deny id="deny-void">
        <void/>
    </deny>
    <all id="only-gold-cause-world">
        <material id="only-gold">gold block</material>
        <cause>world</cause>
    </all>
</filters>
<regions>
    <negative id="void-area">
        <region id="build-area"/>
        <region id="spawns"/>
        <region id="water-edges"/>
    </negative>
    <negative id="not-spawns">
        <union id="spawns">
            <rectangle id="blue-spawn" min="81,-105" max="58,-47"/>
            <rectangle id="green-spawn" min="-80,106" max="-57,48"/>
        </union>
    </negative>
    <union id="build-area">
        <rectangle min="-59,-50" max="-20,38"/> <!-- West Lane -->
        <rectangle min="60,51" max="21,-37"/> <!-- East Lane -->
    </union>
    <union id="water-edges">
        <union id="water-edge-blue">
            <rectangle min="-79,-97" max="-80,-58"/> 
            <circle center="-55,-97" radius="24"/>
            <rectangle min="-78,-103" max="-79,-102"/>
            <rectangle min="-77,-106" max="-78,-105"/>
            <rectangle min="-75,-110" max="-76,-109"/>
            <rectangle min="-67,-118" max="-68,-117"/>
            <rectangle min="-63,-120" max="-64,-119"/>
            <rectangle min="-60,-121" max="-61,-120"/>
            <rectangle min="69,-122" max="-55,-121"/>
            <circle center="66.5,-107.5" radius="14"/>
            <rectangle min="81,-110" max="80,-18"/>
            <complement>
                <rectangle min="80,-37" max="60,-17"/>
                <half origin="79,0,-17" normal="-1,0,1"/>
            </complement>
            <rectangle min="21,-36" max="-14,-35"/>
            <rectangle min="11,-35" max="-11,-34"/>
            <rectangle min="-14,-37" max="-17,-36"/>
            <rectangle min="-17,-38" max="-20,-37"/>
            <complement>
                <rectangle min="-58,-58" max="-79,-51"/>
                <half origin="-61,0,-51" normal="-1,0,2.85"/> 
            </complement>
        </union>
        <union id="water-edge-green">
            <rectangle min="80,98" max="81,59"/> 
            <circle center="56,98" radius="24"/>
            <rectangle min="79,104" max="80,103"/>
            <rectangle min="78,107" max="79,106"/>
            <rectangle min="76,111" max="77,110"/>
            <rectangle min="68,119" max="69,118"/>
            <rectangle min="64,121" max="65,120"/>
            <rectangle min="61,122" max="62,121"/>
            <rectangle min="-68,123" max="56,122"/>
            <circle center="-65.5,108.5" radius="14"/>
            <rectangle min="-80,111" max="-79,19"/>
            <complement>
                <rectangle min="-79,38" max="-59,18"/>
                <half origin="-78,0,18" normal="1,0,-1"/>
            </complement>
            <rectangle min="-20,37" max="15,36"/>
            <rectangle min="-10,36" max="12,35"/>
            <rectangle min="15,38" max="18,37"/>
            <rectangle min="18,39" max="21,38"/>
            <complement>
                <rectangle min="59,59" max="80,52"/>
                <half origin="62,0,52" normal="1,0,-2.85"/> 
            </complement>
        </union>
    </union>
    <union id="spawners">
        <point id="gold-point-west">-34.5,12,-12.5</point>
        <cuboid id="gold-point-west-platform" min="-33,14,-14" max="-36,11,-11"/>
        <point id="gold-point-east">35.5,12,13.5</point>
        <cuboid id="gold-point-east-platform" min="34,14,15" max="37,11,12"/>
    </union>
    <apply block-place="only-gold-cause-world" block-break="only-gold" region="spawns" message="You may not edit spawn!"/>
    <apply enter="only-blue" region="blue-spawn" message="You may not enter the opponent's spawn!"/>
    <apply enter="only-green" region="green-spawn" message="You may not enter the opponent's spawn!"/>
    <apply block="never" region="spawners" message="You may not edit the golden apple spawner!"/>
    <apply block-place="deny-void" region="void-area" message="You may not build in the void!"/>
</regions>
<spawners player-region="everywhere">
    <spawner spawn-region="gold-point-west" max-entities="1" delay="10s">
        <item amount="1" material="golden apple"/>
    </spawner>
    <spawner spawn-region="gold-point-east" max-entities="1" delay="10s">
        <item amount="1" material="golden apple"/>
    </spawner>
</spawners>
<renewables>
    <renewable sound="false" region="spawns" renew-filter="only-gold"/>
</renewables>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
    <tool>iron spade</tool>
    <tool>shears</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>arrow</item>
    <item>seeds</item>
    <item>flower pot</item>
    <item>melon seeds</item>
    <item>melon block</item>
    <item>melon stem</item>
    <item>water lily</item>
    <item>sapling:3</item>
    <item>book</item>
    <item>wheat</item>
    <item>hay block</item>
    <item>item frame</item> 
    <item>string</item>
    <item>prismarine crystals</item>
    <item>coal block</item>
    <item>emerald block</item>
    <item>obsidian</item>
    <item>beacon</item>
</itemremove>
<itemkeep>
    <item>vine</item>
    <item>wood</item>
    <item>stained clay</item>
    <item>golden apple</item>
    <item>torch</item>
    <item>water bucket</item>
    <item>bucket</item>
</itemkeep>
<item-mods>
    <rule>
        <match>
            <material>gold sword</material>
            <material>gold helmet</material>
            <material>gold chestplate</material>
            <material>gold leggings</material>
            <material>gold boots</material>
        </match>
        <modify unbreakable="true"/>
    </rule>
</item-mods>
<crafting>
	<disable>gold pickaxe</disable>
    <disable>coal block</disable>
</crafting>
<maxbuildheight>27</maxbuildheight>
<hunger>
    <depletion>off</depletion>
</hunger>
<kill-rewards>
    <kill-reward>
        <item material="golden apple"/>
        <item material="wood" amount="16" damage="3"/>
        <item material="vine" amount="16"/>
    </kill-reward>
</kill-rewards>
</map>
