<actions>
    <action id="activate_eff_i" scope="match">
        <set var="eff_i" value="1" />
        <switch-scope inner="player">
            <enchant-item ignore-metadata="true" enchantment="efficiency" level="1">
                <find material="diamond pickaxe"/>
            </enchant-item>
        </switch-scope>
    </action>
    <action id="activate_eff_ii" scope="match">
        <set var="eff_i" value="0" />
        <set var="eff_ii" value="1" />
        <switch-scope inner="player">
            <enchant-item ignore-metadata="true" enchantment="efficiency" level="2">
                <find material="diamond pickaxe"/>
            </enchant-item>
        </switch-scope>
    </action>
    <action id="activate_eff_iii" scope="match">
        <set var="eff_i" value="0" />
        <set var="eff_ii" value="0" />
        <set var="eff_iii" value="1" />
        <switch-scope inner="player">
            <enchant-item ignore-metadata="true" enchantment="efficiency" level="3">
                <find material="diamond pickaxe"/>
            </enchant-item>
        </switch-scope>
    </action>
    <action id="activate_eff_iv" scope="match">
