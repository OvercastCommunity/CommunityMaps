        <kit filter="eff_iv=1">
            <item slot="2" material="diamond pickaxe" unbreakable="true">
                <enchantment level="4">efficiency</enchantment>
            </item>
        </kit>
        <kit filter="eff_v=1">
            <item slot="2" material="diamond pickaxe" unbreakable="true">
                <enchantment level="5">efficiency</enchantment>
            </item>
        </kit>
    </kit>
</kits>
