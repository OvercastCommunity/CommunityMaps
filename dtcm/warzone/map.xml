        <set var="eff_ii" value="0" />
        <set var="eff_iii" value="0" />
        <set var="eff_iv" value="1" />
        <switch-scope inner="player">
            <enchant-item ignore-metadata="true" enchantment="efficiency" level="4">
                <find material="diamond pickaxe"/>
            </enchant-item>
        </switch-scope>
    </action>
    <action id="activate_eff_v" scope="match">
        <set var="eff_i" value="0" />
        <set var="eff_ii" value="0" />
        <set var="eff_iii" value="0" />
        <set var="eff_iv" value="0" />
        <set var="eff_v" value="1" />
        <switch-scope inner="player">
            <enchant-item ignore-metadata="true" enchantment="efficiency" level="5">
                <find material="diamond pickaxe"/>
            </enchant-item>
        </switch-scope>
    </action>
</actions>
