</authors>
<contributors>
    <contributor uuid="621b2aa8-4e5f-4ed8-bd36-0ae2f8952a06" contribution="feedback"/> <!-- Strangey -->
</contributors>
<teams>
    <team id="blue-team" color="blue" max="25">Blue</team>
    <team id="red-team" color="red" max="25">Red</team>
    <team id="yellow-team" color="yellow" max="25">Yellow</team>
    <team id="green-team" color="green" max="25">Green</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" material="iron sword" unbreakable="true"/>
        <item slot="1" material="bow" unbreakable="true" enchantment="arrow infinite"/>
        <item slot="2" material="iron pickaxe" unbreakable="true"/>
        <item slot="3" material="iron axe" unbreakable="true"/>
        <item slot="4" material="wood" amount="64"/>
        <item slot="6" material="vine" amount="24"/>
        <item slot="8" material="golden apple"/>
        <item slot="28" material="arrow"/>
        <item slot="30" material="shears" unbreakable="true"/>
        <item slot="29" material="iron spade" unbreakable="true"/>
        <helmet material="iron helmet" unbreakable="true"/>
        <chestplate material="chainmail chestplate" unbreakable="true"/>
        <effect duration="oo" amplifier="100">damage resistance</effect>
    </kit>
    <kit force="true" id="reset-resitance-kit">
        <effect duration="0" amplifier="0">damage resistance</effect>
    </kit>
    <kit id="blue-kit" parents="spawn-kit">
        <item slot="4" material="stained clay" damage="3" amount="32"/>
        <leggings material="leather leggings" color="3498DB" unbreakable="true"/>
        <boots material="leather boots" color="3498DB" unbreakable="true"/>
    </kit>
    <kit id="red-kit" parents="spawn-kit">
        <item slot="4" material="stained clay" damage="6" amount="32"/>
        <leggings material="leather leggings" color="E74C3C" unbreakable="true"/>
        <boots material="leather boots" color="E74C3C" unbreakable="true"/>
    </kit>
    <kit id="yellow-kit" parents="spawn-kit">
        <item slot="4" material="stained clay" damage="4" amount="32"/>
        <leggings material="leather leggings" color="F1C40F" unbreakable="true"/>
        <boots material="leather boots" color="F1C40F" unbreakable="true"/>
    </kit>
    <kit id="green-kit" parents="spawn-kit">
        <item slot="4" material="stained clay" damage="5" amount="32"/>
        <leggings material="leather leggings" color="28B463" unbreakable="true"/>
        <boots material="leather boots" color="28B463" unbreakable="true"/>
    </kit>
</kits>
<spawns>
    <spawn team="blue-team" kit="blue-kit" region="blue-spawn" yaw="110"/>
    <spawn team="red-team" kit="red-kit" region="red-spawn" yaw="-160"/>
    <spawn team="yellow-team" kit="yellow-kit" region="yellow-spawn" yaw="-70"/>
    <spawn team="green-team" kit="green-kit" region="green-spawn" yaw="20"/>
    <default region="obs-spawn" yaw="-90"/>
</spawns>
<wools craftable="false">
    <!-- blue team -->
    <wool team="blue-team" monument="blue-team-red-wool" color="red" location="-23,19.2,95"/>
    <wool team="blue-team" monument="blue-team-magenta-wool" color="magenta" location="-105,10.2,67"/>
    <wool team="blue-team" monument="blue-team-yellow-wool" color="yellow" location="-95,19.2,-23"/>
    <wool team="blue-team" monument="blue-team-orange-wool" color="orange" location="-67,10.2,-105"/>
    <wool team="blue-team" monument="blue-team-green-wool" color="green" location="23,19.2,-95"/>
    <wool team="blue-team" monument="blue-team-lime-wool" color="lime" location="105,10.2,-67"/>
    <!-- red team -->
    <wool team="red-team" monument="red-team-yellow-wool" color="yellow" location="-95,19.2,-23"/>
    <wool team="red-team" monument="red-team-orange-wool" color="orange" location="-67,10.2,-105"/>
    <wool team="red-team" monument="red-team-green-wool" color="green" location="23,19.2,-95"/>
    <wool team="red-team" monument="red-team-lime-wool" color="lime" location="105,10.2,-67"/>
    <wool team="red-team" monument="red-team-blue-wool" color="blue" location="95,19.2,23"/>
    <wool team="red-team" monument="red-team-cyan-wool" color="cyan" location="67,10.2,105"/>
    <!-- yellow team -->
    <wool team="yellow-team" monument="yellow-team-green-wool" color="green" location="23,19.2,-95"/>
    <wool team="yellow-team" monument="yellow-team-lime-wool" color="lime" location="105,10.2,-67"/>
    <wool team="yellow-team" monument="yellow-team-blue-wool" color="blue" location="95,19.2,23"/>
    <wool team="yellow-team" monument="yellow-team-cyan-wool" color="cyan" location="67,10.2,105"/>
    <wool team="yellow-team" monument="yellow-team-red-wool" color="red" location="-23,19.2,95"/>
    <wool team="yellow-team" monument="yellow-team-magenta-wool" color="magenta" location="-105,10.2,67"/>
    <!-- green team -->
    <wool team="green-team" monument="green-team-blue-wool" color="blue" location="95,19.2,23"/>
    <wool team="green-team" monument="green-team-cyan-wool" color="cyan" location="67,10.2,105"/>
    <wool team="green-team" monument="green-team-red-wool" color="red" location="-23,19.2,95"/>
    <wool team="green-team" monument="green-team-magenta-wool" color="magenta" location="-105,10.2,67"/>
    <wool team="green-team" monument="green-team-yellow-wool" color="yellow" location="-95,19.2,-23"/>
    <wool team="green-team" monument="green-team-orange-wool" color="orange" location="-67,10.2,-105"/>
</wools>
<filters>
    <not id="not-blue">
        <team id="only-blue">blue-team</team>
    </not>
    <not id="not-red">
        <team id="only-red">red-team</team>
    </not>
    <not id="not-yellow">
        <team id="only-yellow">yellow-team</team>
    </not>
    <not id="not-green">
        <team id="only-green">green-team</team>
    </not>
    <not id="not-void">
        <void/>
    </not>
    <not id="not-void-and-web">
        <all>
            <material>web</material>
            <void/>
        </all>
    </not>
    <all id="blues-woolrooms-filter">
        <not>
            <team>blue-team</team>
        </not>
        <filter id="woolrooms-filter"/>
    </all>
    <all id="reds-woolrooms-filter">
        <not>
            <team>red-team</team>
        </not>
        <filter id="woolrooms-filter"/>
    </all>
    <all id="yellows-woolrooms-filter">
        <not>
            <team>yellow-team</team>
        </not>
        <filter id="woolrooms-filter"/>
    </all>
    <all id="greens-woolrooms-filter">
        <not>
            <team>green-team</team>
        </not>
        <filter id="woolrooms-filter"/>
    </all>
    <any id="woolrooms-filter">
        <material>web</material>
        <material>wood</material>
        <material>stained clay</material>
        <material>iron block</material>
        <material>wool:9</material>
        <material>wool:11</material>
        <material>wool:2</material>
        <material>wool:14</material>
        <material>wool:4</material>
        <material>wool:1</material>
        <material>wool:5</material>
        <material>wool:13</material>
    </any>
    <deny id="deny-wools">
        <any>
            <material>wool:9</material>
            <material>wool:11</material>
            <material>wool:2</material>
            <material>wool:14</material>
            <material>wool:4</material>
            <material>wool:1</material>
            <material>wool:5</material>
            <material>wool:13</material>
        </any>
    </deny>
    <deny id="deny-woolroom-use">
        <material>dispenser</material>
    </deny>
    <material id="only-wool-9">wool:9</material>
    <material id="only-wool-11">wool:11</material>
    <material id="only-wool-2">wool:2</material>
    <material id="only-wool-14">wool:14</material>
    <material id="only-wool-4">wool:4</material>
    <material id="only-wool-1">wool:1</material>
    <material id="only-wool-5">wool:5</material>
    <material id="only-wool-13">wool:13</material>
    <any id="only-iron-or-diamond-ore">
        <material id="only-iron">iron block</material>
        <material>diamond ore</material>
    </any>
    <all id="blue-and-stained-clay">
        <team>blue-team</team>
        <material>stained clay</material>
    </all>
    <all id="red-and-stained-clay">
        <team>red-team</team>
        <material>stained clay</material>
    </all>
    <all id="yellow-and-stained-clay">
        <team>yellow-team</team>
        <material>stained clay</material>
    </all>
    <all id="green-and-stained-clay">
        <team>green-team</team>
        <material>stained clay</material>
    </all>
    <deny id="deny-lava">
        <any>
            <material>lava</material>
            <material>stationary lava</material>
        </any>
    </deny>
</filters>
<regions>
    <!-- spawns -->
    <cylinder id="blue-spawn" base="117,45,75" radius="0.5" height="0"/>
    <cylinder id="red-spawn" base="-75,45,117" radius="0.5" height="0"/>
    <cylinder id="yellow-spawn" base="-117,45,-75" radius="0.5" height="0"/>
    <cylinder id="green-spawn" base="75,45,-117" radius="0.5" height="0"/>
    <cylinder id="obs-spawn" base="-75.5,47.2,0.5" radius="2" height="0"/>
    <!-- victory monuments -->
    <!-- blue -->
    <block id="blue-team-red-wool">112,20,77</block>
    <block id="blue-team-magenta-wool">111,20,77</block>
    <block id="blue-team-yellow-wool">110,20,76</block>
    <block id="blue-team-orange-wool">109,20,75</block>
    <block id="blue-team-green-wool">108,20,75</block>
    <block id="blue-team-lime-wool">107,20,74</block>
    <!-- red -->
    <block id="red-team-yellow-wool">-78,20,112</block>
    <block id="red-team-orange-wool">-78,20,111</block>
    <block id="red-team-green-wool">-77,20,110</block>
    <block id="red-team-lime-wool">-76,20,109</block>
    <block id="red-team-blue-wool">-76,20,108</block>
    <block id="red-team-cyan-wool">-75,20,107</block>
    <!-- yellow -->
    <block id="yellow-team-green-wool">-113,20,-78</block>
    <block id="yellow-team-lime-wool">-112,20,-78</block>
    <block id="yellow-team-blue-wool">-111,20,-77</block>
    <block id="yellow-team-cyan-wool">-110,20,-76</block>
    <block id="yellow-team-red-wool">-109,20,-76</block>
    <block id="yellow-team-magenta-wool">-108,20,-75</block>
    <!-- green -->
    <block id="green-team-blue-wool">77,20,-113</block>
    <block id="green-team-cyan-wool">77,20,-112</block>
    <block id="green-team-red-wool">76,20,-111</block>
    <block id="green-team-magenta-wool">75,20,-110</block>
    <block id="green-team-yellow-wool">75,20,-109</block>
    <block id="green-team-orange-wool">74,20,-108</block>
    <!-- wool spawns -->
    <point id="blue-wool-spawn">95,19.2,23</point>
    <point id="cyan-wool-spawn">67,10.2,105</point>
    <point id="red-wool-spawn">-23,19.2,95</point>
    <point id="magenta-wool-spawn">-105,10.2,67</point>
    <point id="yellow-wool-spawn">-95,19.2,-23</point>
    <point id="orange-wool-spawn">-67,10.2,-105</point>
    <point id="green-wool-spawn">23,19.2,-95</point>
    <point id="lime-wool-spawn">105,10.2,-67</point>
    <!-- beacon protection -->
    <union id="beacons">
        <circle id="blue-beacon" center="101.5,16.5" radius="0.5"/>
        <circle id="cyan-beacon" center="65.5,106.5" radius="0.5"/>
        <circle id="red-beacon" center="-16.5,101.5" radius="0.5"/>
        <circle id="magenta-beacon" center="-106.5,65.5" radius="0.5"/>
        <circle id="yellow-beacon" center="-101.5,-16.5" radius="0.5"/>
        <circle id="orange-beacon" center="-65.5,-106.5" radius="0.5"/>
        <circle id="green-beacon" center="16.5,-101.5" radius="0.5"/>
        <circle id="lime-beacon" center="106.5,-65.5" radius="0.5"/>
    </union>
    <!-- misc -->
    <negative id="not-bases">
        <union id="bases">
            <rectangle id="blue-base" min="104,60" max="129,110"/>
            <rectangle id="red-base" min="-60,104" max="-110,129"/>
            <rectangle id="yellow-base" min="-104,-60" max="-129,-110"/>
            <rectangle id="green-base" min="60,-104" max="110,-129"/>
        </union>
    </negative>
    <union id="woolrooms">
        <union id="blues-woolrooms"> <!-- blue defends -->
            <rectangle id="blue-woolroom" min="86,14" max="104,34"/>
            <rectangle id="cyan-woolroom" min="57,95" max="75,115"/>
        </union>
        <union id="reds-woolrooms"> <!-- red defends -->
            <rectangle id="red-woolroom" min="-14,86" max="-34,104"/>
            <rectangle id="magenta-woolroom" min="-95,57" max="-115,75"/>
        </union>
        <union id="yellows-woolrooms"> <!-- yellow defends -->
            <rectangle id="yellow-woolroom" min="-86,-14" max="-104,-34"/>
            <rectangle id="orange-woolroom" min="-57,-95" max="-75,-115"/>
        </union>
        <union id="greens-woolrooms"> <!-- green defends -->
            <rectangle id="green-woolroom" min="14,-86" max="34,-104"/>
            <rectangle id="lime-woolroom" min="95,-57" max="115,-75"/>
        </union>
    </union>
    <negative id="not-build-area">
        <union id="build-area">
            <rectangle min="-65,-65" max="65,65"/> <!-- mid -->
        </union>
    </negative>
    <!-- applicators -->
    <apply enter="only-blue" region="blue-base" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-red" region="red-base" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-yellow" region="yellow-base" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-green" region="green-base" message="You may not enter the enemy's spawn!"/>
    <apply enter="not-blue" region="blues-woolrooms" message="You may not enter your own wool room!"/>
    <apply enter="not-red" region="reds-woolrooms" message="You may not enter your own wool room!"/>
    <apply enter="not-yellow" region="yellows-woolrooms" message="You may not enter your own wool room!"/>
    <apply enter="not-green" region="greens-woolrooms" message="You may not enter your own wool room!"/>
    <apply block-place="deny-wools" region="everywhere" message="You may not place safty wools!"/>
    <apply block-physics="deny-lava" region="everywhere"/>
    <apply block-place="never" block-break="only-iron-or-diamond-ore" region="bases" message="You may not edit spawn!"/>
    <apply block-place="never" region="beacons" message="You may not edit the wool room!"/>
    <apply block="blues-woolrooms-filter" region="blues-woolrooms" message="You may not edit the wool room!"/>
    <apply block="reds-woolrooms-filter" region="reds-woolrooms" message="You may not edit the wool room!"/>
    <apply block="yellows-woolrooms-filter" region="yellows-woolrooms" message="You may not edit the wool room!"/>
    <apply block="greens-woolrooms-filter" region="greens-woolrooms" message="You may not edit the wool room!"/>
    <apply block-place="not-void" block-break="not-void-and-web" region="not-build-area" message="You may not edit the void"/>
    <apply use="deny-woolroom-use" region="woolrooms"/>
    <apply kit="reset-resitance-kit" region="not-bases"/>
</regions>
<block-drops>
    <rule wrongtool="false" region="woolrooms" filter="only-iron">
        <drops>
            <item material="iron block"/>
        </drops>
        <replacement>iron block</replacement>
    </rule>
    <!-- stained clay -->
    <rule wrongtool="false" region="everywhere" filter="blue-and-stained-clay">
        <drops>
            <item damage="3" material="stained clay"/>
        </drops>
    </rule>
    <rule wrongtool="false" region="everywhere" filter="red-and-stained-clay">
        <drops>
            <item damage="6" material="stained clay"/>
        </drops>
    </rule>
    <rule wrongtool="false" region="everywhere" filter="yellow-and-stained-clay">
        <drops>
            <item damage="4" material="stained clay"/>
        </drops>
    </rule>
    <rule wrongtool="false" region="everywhere" filter="green-and-stained-clay">
        <drops>
            <item damage="5" material="stained clay"/>
        </drops>
    </rule>
    <!-- wools -->
    <rule wrongtool="false" region="woolrooms" filter="only-wool-9">
        <drops>
            <item damage="9" material="wool"/>
        </drops>
        <replacement>wool:9</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-11">
        <drops>
            <item damage="11" material="wool"/>
        </drops>
        <replacement>wool:11</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-2">
        <drops>
            <item damage="2" material="wool"/>
        </drops>
        <replacement>wool:2</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-14">
        <drops>
            <item damage="14" material="wool"/>
        </drops>
        <replacement>wool:14</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-4">
        <drops>
            <item damage="4" material="wool"/>
        </drops>
        <replacement>wool:4</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-1">
        <drops>
            <item damage="1" material="wool"/>
        </drops>
        <replacement>wool:1</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-5">
        <drops>
            <item damage="5" material="wool"/>
        </drops>
        <replacement>wool:5</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-13">
        <drops>
            <item damage="13" material="wool"/>
        </drops>
        <replacement>wool:13</replacement>
    </rule>
</block-drops>
<spawners>
    <spawner spawn-region="blue-wool-spawn" player-region="blue-woolroom" delay="3s">
        <item damage="11" amount="3" material="wool"/>
    </spawner>
    <spawner spawn-region="cyan-wool-spawn" player-region="cyan-woolroom" delay="3s">
        <item damage="9" amount="3" material="wool"/>
    </spawner>
    <spawner spawn-region="red-wool-spawn" player-region="red-woolroom" delay="3s">
        <item damage="14" amount="3" material="wool"/>
    </spawner>
    <spawner spawn-region="magenta-wool-spawn" player-region="magenta-woolroom" delay="3s">
        <item damage="2" amount="3" material="wool"/>
    </spawner>
    <spawner spawn-region="yellow-wool-spawn" player-region="yellow-woolroom" delay="3s">
        <item damage="4" amount="3" material="wool"/>
    </spawner>
    <spawner spawn-region="orange-wool-spawn" player-region="orange-woolroom" delay="3s">
        <item damage="1" amount="3" material="wool"/>
    </spawner>
    <spawner spawn-region="green-wool-spawn" player-region="green-woolroom" delay="3s">
        <item damage="13" amount="3" material="wool"/>
    </spawner>
    <spawner spawn-region="lime-wool-spawn" player-region="lime-woolroom" delay="3s">
        <item damage="5" amount="3" material="wool"/>
    </spawner>
</spawners>
<item-mods>
    <rule>
        <match>
            <material>diamond chestplate</material>
            <material>bow</material>
            <material>iron helmet</material>
            <material>iron chestplate</material>
            <material>iron leggings</material>
            <material>iron boots</material>
        </match>
        <modify unbreakable="true"/>
    </rule>
</item-mods>
