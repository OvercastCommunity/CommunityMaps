  </spawns>
  <respawn spectate="true"/>
  <kits>
    <kit id="spawn">
      <item slot="0" unbreakable="true" name="`bChunchunmaru" lore="One punch kill!" enchantment="damage all:10;knockback:10" material="diamond sword"/>
      <item slot="1" unbreakable="true" name="`bStrongest Bow" lore="One shot kill!" enchantment="arrow damage:10;arrow knockback:10" material="bow"/>
      <item slot="8" amount="8" material="arrow"/>
      <helmet locked="true" name="`f`lI wanna fly!" material="glass"/>
      <potion duration="oo">night vision</potion>
      <potion duration="oo">fire_resistance</potion>
      <potion duration="oo" amplifier="6">jump</potion>
      <potion amplifier="50" duration="3">damage resistance</potion>
      <potion amplifier="10" duration="oo">regeneration</potion>
      <potion amplifier="50" duration="oo">saturation</potion>
      <fly/>
      <walk-speed>1.2</walk-speed>
    </kit>
    <kit id="obs">
      <helmet name="`b`lShall we fly?" material="command"/>
      <potion duration="oo">night vision</potion>
    </kit>
    <kit id="tp" force="true">
      <potion duration="1" amplifier="50">damage resistance</potion>
    </kit>
  </kits>
