<rules>
    <rule>Teaming is allowed.</rule>
    <rule>Spawnkilling is allowed.</rule>
    <rule>Every team has potion effects on its own soil.</rule>
    <rule>Falldamage is disabled.</rule>
    <rule>Hunger is disabled.</rule>
</rules>
<teams>
    <team id="red-team" color="dark red" max="5">Red</team>
    <team id="orange-team" color="gold" max="5">Orange</team>
    <team id="yellow-team" color="yellow" max="5">Yellow</team>
    <team id="lime-team" color="green" max="5">Lime</team>
    <team id="green-team" color="dark green" max="5">Green</team>
    <team id="cyan-team" color="dark aqua" max="5">Cyan</team>
    <team id="blue-team" color="dark blue" max="5">Blue</team>
    <team id="magenta-team" color="light purple" max="5">Magenta</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" material="iron sword" unbreakable="true"/>
        <item slot="1" material="bow" unbreakable="true" enchantments="infinity"/>
        <item slot="2" material="diamond pickaxe" unbreakable="true" enchantments="efficiency:3"/>
        <item slot="3" material="iron axe" unbreakable="true"/>
        <item slot="4" material="leaves" damage="3" amount="64"/>
        <item slot="5" material="stained clay" team-color="true" amount="32"/>
        <item slot="6" material="vine" amount="48"/>
        <item slot="8" material="golden apple" amount="3"/>
        <item slot="28" material="arrow"/>
        <item slot="29" material="shears" unbreakable="true"/>
        <item slot="30" material="stone spade" unbreakable="true"/>
        <helmet unbreakable="true" material="chainmail helmet"/>
        <chestplate unbreakable="true" enchantments="projectile_protection:2" material="chainmail chestplate"/>
        <leggings color="993333" unbreakable="true" material="leather leggings"/>
        <boots color="993333" unbreakable="true" enchantments="feather_falling;depth_strider" material="leather boots"/>
        <effect>resistance</effect>
        <effect>strength</effect>
    </kit>
    <kit id="def-kit">
        <effect>speed</effect>
        <effect>resistance</effect>
        <effect>strength</effect>
    </kit>
</kits>
