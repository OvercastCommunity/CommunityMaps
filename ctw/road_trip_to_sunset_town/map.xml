<filters>
    <not id="no-void-break">
        <any>
            <cause>explosion</cause>
            <material>wool</material>
            <void/>
        </any>
    </not>
    <not id="no-void-place">
        <any>
            <material>wool</material>
            <void/>
        </any>
    </not>
    <not id="no-safeties">
        <material>wool</material>
    </not>
    <not id="not-new">
        <effect id="only-new" amplifier="1">saturation</effect>
    </not>
    <not id="not-green">
        <team id="only-green">green</team>
    </not>
    <all id="only-green-new">
        <filter id="only-green"/>
