<portals>
    <portal region="blue-portal-from-spawn" destination="blue-spawn-exit" yaw="@90"/>
    <portal region="red-portal-from-spawn" destination="red-spawn-exit" yaw="@-90"/>
    <portal region="blue-portal-to-spawn" destination="blue-spawn-point" yaw="@90"/>
    <portal region="red-portal-to-spawn" destination="red-spawn-point" yaw="@-90"/>
</portals>
<spawners>
    <spawner player-region="player-region" spawn-region="spawn-region" delay="7">
        <item material="tnt"/>
    </spawner>
</spawners>
<itemkeep>
    <item>iron sword</item>
    <item>bow</item>
    <item>iron pickaxe</item>
    <item>iron axe</item>
    <item>shears</item>
    <item>arrow</item>
    <item>iron spade</item>
    <item>wood:1</item>
    <item>stained glass</item>
    <item>water bucket</item>
    <item>bucket</item>
    <item>golden apple</item>
</itemkeep>
<itemremove>
    <item>cobblestone</item>
    <item>dirt</item>
    <item>string</item>
    <item>wool:11</item>
    <item>wool:14</item>
    <item>glowstone dust</item>
    <item>long grass</item>
    <item>red rose</item>
    <item>yellow flower</item>
    <item>seeds</item>
    <item>sapling</item>
    <item>apple</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
</itemremove>
<block-drops>
    <rule>
        <filter>
            <any>
                <material>wood:1</material>
                <material>stained glass</material>
            </any>
        </filter>
        <drops>
            <item chance="0" material="glass"/>
        </drops>
    </rule>
</block-drops>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
    <tool>arrow</tool>
    <tool>shears</tool>
    <tool>iron spade</tool>
</toolrepair>
<item-mods>
    <rule>
        <match>
            <material>iron helmet</material>
            <material>iron chestplate</material>
            <material>iron leggings</material>
            <material>iron boots</material>
            <material>iron sword</material>
            <material>iron pickaxe</material>
            <material>iron axe</material>
            <material>iron spade</material>
            <material>shears</material>
        </match>
        <modify unbreakable="true"/>
    </rule>
</item-mods>
<crafting>
    <!-- bulkcrafting -->
    <shaped override="false">
        <result material="iron helmet" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron chestplate" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>III</row>
            <row>III</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron leggings" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="bucket" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>.I.</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
</crafting>
<renewables>
    <renewable rate="0.5" sound="false" region="iron-regen" renew-filter="only-iron" replace-filter="only-air" avoid-players="5"/>
</renewables>
<tnt>
    <instantignite>true</instantignite>
    <friendly-defuse>false</friendly-defuse>
    <power>2.5</power>
</tnt>
<hunger>
    <depletion>off</depletion>
</hunger>
<maxbuildheight>45</maxbuildheight>
<kill-rewards>
    <kill-reward>
        <item material="golden apple"/>
        <item material="wood" amount="32" damage="1"/>
    </kill-reward>
    <kill-reward filter="only-blue">
        <item slot="5" material="stained glass" damage="11" amount="16"/>
    </kill-reward>
    <kill-reward filter="only-red">
        <item slot="5" material="stained glass" damage="14" amount="16"/>
    </kill-reward>
</kill-rewards>
</map>
