<map proto="1.4.2">
<name>Black Betty</name>
<version>1.1.0</version>
<phase>development</phase>
<objective>Retrieve your wools!</objective>
<gamemode>ctw</gamemode>
<created>2023-05-01</created>
<authors>
    <author uuid="c25a6f7b-4c42-40da-8cd6-add53f0c84eb"/> <!-- arcadeboss -->
</authors>
<contributors>
    <contributor uuid="510dae51-f405-4cf4-9fb6-6f1fd3f07d69" contribution="Bugs, Design Advice"/> <!-- Zuiuzz -->
</contributors>
<teams>
    <team id="blue-team" color="blue" max="45">Blue</team>
    <team id="red-team" color="red" max="45">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" material="diamond sword" unbreakable="true" enchantment="sharpness:1"/>
        <item slot="1" material="bow" unbreakable="true" enchantment="arrow infinite;power:1"/>
        <item slot="2" material="iron pickaxe" unbreakable="true" enchantment="unbreaking:3"/>
        <item slot="3" material="iron axe" unbreakable="true" enchantment="unbreaking:3"/>
        <item slot="4" material="wood" amount="48"/>
        <item slot="7" material="bucket"/>
        <item slot="8" material="golden apple"/>
        <item slot="9" material="iron ingot" amount="1"/>
        <item slot="34" material="exp bottle" amount="5" prevent-sharing="true"/>
        <item slot="35" material="ink sack" damage="4" amount="3"/> <!--lapis-->
        <item slot="10" material="arrow"/>
        <item slot="31" material="iron spade" unbreakable="true"/>
        <helmet material="diamond helmet" unbreakable="true"/>
        <chestplate material="diamond chestplate" unbreakable="true"/>
        <leggings material="diamond leggings" unbreakable="true"/>
        <boots material="diamond boots" unbreakable="true"/>
        <effect duration="1m" amplifier="1">absorption</effect>
        <effect duration="22s" amplifier="1">regeneration</effect>
        <effect duration="5s" amplifier="255">damage resistance</effect>
    </kit>
    <kit id="blue-kit" parent="spawn-kit">
        <item slot="5" damage="11" material="wool" amount="64"/>
        <item slot="6" damage="11" material="stained glass" amount="16"/>
    </kit>
    <kit id="red-kit" parent="spawn-kit">
        <item slot="5" damage="14" material="wool" amount="64"/>
        <item slot="6" damage="14" material="stained glass" amount="16"/>
    </kit>
</kits>
<spawns>
    <spawn team="blue-team" kit="blue-kit" yaw="90" region="blue-spawn-point"/>
    <spawn team="red-team" kit="red-kit" yaw="270" region="red-spawn-point"/>
    <default yaw="180" region="obs-spawn-point"/>
</spawns>
<wools craftable="false">
    <wool team="blue-team" color="light blue" location="-148,73,116">
        <monument>
            <block location="190,62,4"/>
        </monument>
    </wool>
    <wool team="blue-team" color="cyan" location="-148,73,-115">
