<filters>
    <any id="block-break-void-filter">
        <all>
            <any>
                <material>leaves</material>
                <material>log</material>
            </any>
            <void/>
        </all>
        <not id="block-place-void-filter">
            <void/>
        </not>
    </any>
    <all id="blue-woolroom-filter">
        <not>
            <team id="only-blue">blue-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <all id="purple-woolroom-filter">
        <not>
            <team id="only-purple">purple-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <all id="red-woolroom-filter">
        <not>
            <team id="only-red">red-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <all id="green-woolroom-filter">
        <not>
            <team id="only-green">green-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <all id="yellow-woolroom-filter">
        <not>
            <team id="only-yellow">yellow-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <all id="silver-woolroom-filter">
        <not>
            <team id="only-silver">silver-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <any id="woolroom-filter">
        <material>web</material>
        <material>wood:3</material>
        <material>stained clay</material>
        <material>gold block</material>
        <material>water</material>
        <material>stationary water</material>
        <material>workbench</material>
    </any>
    <material id="only-wool-3">wool:3</material>
    <material id="only-wool-10">wool:10</material>
    <material id="only-wool-14">wool:14</material>
    <material id="only-wool-5">wool:5</material>
    <material id="only-wool-4">wool:4</material>
    <material id="only-wool-8">wool:8</material>
    <allow id="allow-wool">
        <material>wool</material>
    </allow>
    <any id="allow-block-break-spawn">
        <material>leaves</material>
        <material>dead bush</material>
    </any>
    <deny id="deny-gold">
        <material id="only-gold">gold block</material>
    </deny>
</filters>
