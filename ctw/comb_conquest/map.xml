<map proto="1.5.0">
<name>Comb Conquest</name>
<if min-server-version="1.21.11">
    <variant id="default" world="modern"/>
</if>
<version>1.0.1</version>
<objective>Capture the enemy team's wools.</objective>
<gamemode>CTW</gamemode>
<created>2026-02-07</created>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="6863869b-4b8c-4445-b778-a8e016775ae4"/> <!-- Cazadorian -->
</authors>
<teams>
    <team id="blue" color="blue" max="24">Blue</team>
    <team id="red" color="dark red" max="24">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <if min-server-version="1.21.11">
            <item slot="0" unbreakable="true" material="copper sword" enchantment="sharpness"/>
            <item slot="1" unbreakable="true" material="crossbow" enchantment="infinity;quick charge"/>
            <item slot="3" unbreakable="true" material="copper axe" enchantment="efficiency:2"/>
            <item slot="28" unbreakable="true" material="copper hoe"/>
            <item slot="30" unbreakable="true" material="copper shovel" enchantment="efficiency"/>
        </if>
        <unless min-server-version="1.21.11">
            <item slot="0" unbreakable="true" material="stone sword" enchantment="sharpness"/>
            <item slot="1" unbreakable="true" material="bow" enchantment="infinity"/>
            <item slot="3" unbreakable="true" material="iron axe" enchantment="efficiency"/>
            <item slot="30" unbreakable="true" material="iron spade"/>
            <item slot="4" amount="64" material="wood" damage="2"/>
            <item slot="32" amount="64" material="wood" damage="2"/>
        </unless>
        <item slot="2" unbreakable="true" material="diamond pickaxe" enchantment="efficiency"/>
        <item slot="5" material="stained glass" team-color="true" amount="32"/>
        <item slot="7" material="golden apple"/>
        <item slot="8" material="bucket"/>
        <item slot="27" material="arrow"/>
        <item slot="29" unbreakable="true" material="shears"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" team-color="true" material="leather leggings"/>
        <boots unbreakable="true" team-color="true" material="leather boots"/>
        <effect duration="4" amplifier="255">damage resistance</effect>
    </kit>
    <if min-server-version="1.21.11">
        <kit id="red-kit" parents="spawn-kit">
            <item slot="4" amount="64" material="mangrove planks"/>
            <item slot="32" amount="64" material="mangrove planks"/>
        </kit>
        <kit id="blue-kit" parents="spawn-kit">
            <item slot="4" amount="64" material="warped planks"/>
            <item slot="32" amount="64" material="warped planks"/>
        </kit>
    </if>
</kits>
<spawns>
    <default yaw="90">
        <regions>
            <point>0.5,56,0.5</point>
        </regions>
    </default>
    <if min-server-version="1.21.11">
        <spawn team="blue" kit="blue-kit">
            <regions yaw="180">
                <point>0.5,12,79.5</point>
            </regions>
        </spawn>
    </if>
    <unless min-server-version="1.21.11">
        <spawn team="blue" kit="spawn-kit">
            <regions yaw="180">
                <point>0.5,12,79.5</point>
            </regions>
        </spawn>
    </unless>
    <if min-server-version="1.21.11">
        <spawn team="red" kit="red-kit">
            <regions yaw="0">
                <point>0.5,12,-78.5</point>
            </regions>
        </spawn>
    </if>
    <unless min-server-version="1.21.11">
        <spawn team="red" kit="spawn-kit">
            <regions yaw="0">
                <point>0.5,12,-78.5</point>
            </regions>
        </spawn>
    </unless>
</spawns>
<wools craftable="false">
    <wool team="blue" color="blue" location="59.5,12,-78.5">
        <monument>
            <block>1,13,86</block>
        </monument>
    </wool>
    <wool team="blue" color="cyan" location="-58.5,12,-78.5">
        <monument>
            <block>-1,13,86</block>
        </monument>
    </wool>
    <wool team="red" color="orange" location="-58.5,12,79.5">
        <monument>
            <block>-1,13,-86</block>
        </monument>
    </wool>
    <wool team="red" color="green" location="59.5,12,79.5">
        <monument>
            <block>1,13,-86</block>
        </monument>
    </wool>
</wools>
<filters>
    <not id="deny-void">
        <void/>
    </not>
    <all id="blue-in-wr">
        <filter id="deny-void"/>
        <not>
            <any>
                <team id="only-red">red</team>
                <filter id="wr-filter"/>
            </any>
        </not>
    </all>
    <all id="red-in-wr">
        <filter id="deny-void"/>
        <not>
            <any>
                <team id="only-blue">blue</team>
                <filter id="wr-filter"/>
            </any>
        </not>
    </all>
    <any id="wr-filter">
        <if min-server-version="1.21.11">
            <material>barrel</material>
        </if>
        <unless min-server-version="1.21.11">
            <material>chest</material>
        </unless>
    </any>
    <material id="only-iron">iron block</material>
    <material id="only-air">air</material>
    <deny id="deny-players">
        <participating/>
    </deny>
</filters>
<regions>
    <above id="heightlimit" y="35"/>
    <union id="wool-rooms">
        <union id="blue-wool-rooms">
            <rectangle id="orange-wool-room" min="-48,71" max="-67,88"/> <!-- blue defends -->
            <rectangle id="green-wool-room" min="49,71" max="68,88"/> <!-- blue defends -->
        </union>
        <union id="red-wool-rooms">
            <rectangle id="cyan-wool-room" min="-48,-70" max="-67,-87"/> <!-- red defends -->
            <rectangle id="blue-wool-room" min="49,70" max="68,-87"/> <!-- red defends -->
        </union>
    </union>
    <union id="spawn-protection">
        <rectangle id="red-protection" min="22,-62" max="-21,-88"/>
        <rectangle id="blue-protection" min="22,63" max="-21,89"/>
    </union>
    <union id="build-regions">
        <rectangle id="middle" min="-33,-46" max="34,47"/>
    </union>
    <complement id="void-area">
        <everywhere/>
        <region id="spawn-protection"/>
        <region id="build-regions"/>
    </complement>
    <union id="spawners">
        <point id="blue-spawner">59.5,12,-78.5</point>
        <point id="cyan-spawner">-58.5,12,-78.5</point>
        <point id="orange-spawner">-58.5,12,79.5</point>
        <point id="green-spawner">59.5,12,79.5</point>
    </union>
    <union id="gapple-spawner">
        <point id="gap-point">0.5,10,0.5</point>
        <cylinder id="gap-player" base="0.5,10,0.5" radius="3" height="3"/>
        <cuboid id="gap-protection" min="0,9,0" max="1,11,1"/>
    </union>
    <apply enter="only-blue" region="blue-protection" message="You may not enter the opponent's spawn!"/>
    <apply enter="only-red" region="red-protection" message="You may not enter the opponent's spawn!"/>
    <apply enter="only-blue" region="red-wool-rooms" message="You may not enter your own wool room!"/>
    <apply enter="only-red" region="blue-wool-rooms" message="You may not enter your own wool room!"/>
    <apply block="never" use="never" region="heightlimit">
        <message>{"translate":"match.maxBuildHeight"}</message>
    </apply>
    <apply block="blue-in-wr" use="only-blue" region="red-wool-rooms" message="You may not interact with this block!"/>
    <apply block="red-in-wr" use="only-red" region="blue-wool-rooms" message="You may not interact with this block!"/>
    <apply block-place="deny-void" region="void-area" message="You may not build in the void area!"/>
    <apply block="never" region="gap-protection" message="You may not break the spawner!"/>
    <apply block-break="only-iron" block-place="deny-players" region="spawn-protection" message="You may not modify the spawns!"/>
</regions>
<spawners>
    <spawner spawn-region="gap-point" player-region="gap-player" delay="8s">
        <item material="golden apple" amount="1"/>
    </spawner>
    <spawner spawn-region="blue-spawner" player-region="blue-wool-room" max-entities="16" delay="2s">
        <item amount="1" material="wool" damage="11"/>
    </spawner>
    <spawner spawn-region="cyan-spawner" player-region="cyan-wool-room" max-entities="16" delay="2s">
        <item amount="1" material="wool" damage="9"/>
    </spawner>
    <spawner spawn-region="orange-spawner" player-region="orange-wool-room" max-entities="16" delay="2s">
        <item amount="1" material="wool" damage="1"/>
    </spawner>
    <spawner spawn-region="green-spawner" player-region="green-wool-room" max-entities="16" delay="2s">
        <item amount="1" material="wool" damage="13"/>
    </spawner>
</spawners>
<toolrepair>
    <if min-server-version="1.21.11">
        <tool>copper sword</tool>
        <tool>copper axe</tool>
        <tool>copper shovel</tool>
        <tool>copper hoe</tool>
        <tool>crossbow</tool>
    </if>
    <unless min-server-version="1.21.11">
        <tool>stone sword</tool>
        <tool>iron axe</tool>
        <tool>iron spade</tool>
        <tool>bow</tool>
    </unless>
    <tool>diamond pickaxe</tool>
    <tool>shears</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
    <if min-server-version="1.21.11">
        <item>azalea</item>
        <item>yellow concrete</item>
        <item>honeycomb block</item>
        <item>warped stairs</item>
        <item>warped slab</item>
        <item>mangrove stairs</item>
        <item>mangrove slab</item>
        <item>flowering azalea</item>
        <item>honey block</item>
        <item>shroomlight</item>
    </if>
    <unless min-server-version="1.21.11">
        <item>glowstone dust</item>
        <item>stained clay:4</item>
        <item>birch wood stairs</item>
        <item>wood step:2</item>
        <item>sapling</item>
    </unless>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>gold block</item>
    <item>stained clay</item>
    <item>dark oak stairs</item>
    <item>wood step:5</item>
    <item>string</item>
</itemremove>
<block-drops>
    <if min-server-version="1.21.11">
        <rule>
            <filter>
                <material>mangrove planks</material>
            </filter>
            <drops>
                <item chance="0" material="mangrove planks"/>
            </drops>
        </rule>
        <rule>
            <filter>
                <material>warped planks</material>
            </filter>
            <drops>
                <item chance="0" material="warped planks"/>
            </drops>
        </rule>
    </if>
    <unless min-server-version="1.21.11">
        <rule>
            <filter>
                <material>wood</material>
            </filter>
            <drops>
                <item chance="0" material="wood"/>
            </drops>
        </rule>
    </unless>
</block-drops>
<itemkeep>
    <if min-server-version="1.21.11">
        <item>mangrove planks</item>
        <item>warped planks</item>
    </if>
    <unless min-server-version="1.21.11">
        <item>wood:2</item>
    </unless>
    <item>stained glass</item>
    <item>golden apple</item>
    <item>arrow</item>
    <item>bucket</item>
    <item>water bucket</item>
</itemkeep>
<renewables>
    <renewable region="spawn-protection" renew-filter="only-iron" interval="3" grow="false" particles="true" sound="true"/>
</renewables>
<kill-rewards>
    <kill-reward>
        <item amount="8" material="stained glass" team-color="true"/>
        <item amount="12" material="arrow"/>
        <unless min-server-version="1.21.11">
            <item amount="12" material="wood" damage="2"/>
        </unless>
    </kill-reward>
    <if min-server-version="1.21.11">
        <kill-reward filter="only-blue">
            <item amount="12" material="warped planks"/>
        </kill-reward>
        <kill-reward filter="only-red">
            <item amount="12" material="mangrove planks"/>
        </kill-reward>
    </if>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
