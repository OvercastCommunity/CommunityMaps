<?xml version="1.0"?>
<map proto="1.4.0">
<name>NewGen</name>
<version>1.0.0</version>
<phase>development</phase>
<objective>Capture the other team's wools.</objective>
<rules>
	<rule>Cross-teaming is allowed.</rule>
</rules>
<authors>
    <author uuid="30401f63-f5bb-4f24-9a5a-0ecd5f706115" /> <!-- dentmaged -->
    <author uuid="77d845ce-c88a-467d-9880-1db671a18933" /> <!-- GonkfSLE -->
</authors>
<contributors>
    <contributor uuid="621b2aa8-4e5f-4ed8-bd36-0ae2f8952a06" contribution="Feedback" /> <!-- Strangey -->
    <contributor uuid="430ec559-364a-4363-ac7a-2529050440ac" contribution="Feedback" /> <!-- mame_BT -->
</contributors>
<heightlimit>28</heightlimit>
<teams>
    <team id="yellow-team" color="yellow" max="16">Yellow</team>
    <team id="blue-team" color="blue" max="16">Blue</team>
    <team id="green-team" color="green" max="16">Green</team>
    <team id="red-team" color="dark red" max="16">Red</team>
</teams>
<spawns>
    <spawn team="yellow-team" kit="yellow-kit" yaw="0">
        <regions><block>105,6,-58</block></regions>
    </spawn>
    <spawn team="blue-team" kit="blue-kit" yaw="-90">
        <regions><block>197,6,-42</block></regions>
    </spawn>
    <spawn team="green-team" kit="green-kit" yaw="90">
        <regions><block>121,6,-150</block></regions>
    </spawn>
    <spawn team="red-team" kit="red-kit" yaw="-180">
        <regions><block>213,6,-134</block></regions>
    </spawn>
    <default yaw="0">
        <regions><point>159.5,41,-95.9</point></regions>
    </default>
</spawns>
<kits>
    <kit id="base-team" force="true">
        <item slot="0" unbreakable="true" material="iron sword" />
        <item slot="1" unbreakable="true" material="bow" />
        <item slot="2" unbreakable="true" material="iron pickaxe" />
        <item slot="3" unbreakable="true" material="iron axe" />
        <item slot="7">golden apple</item>        
        <item slot="8" amount="64">golden carrot</item>
        <item slot="28" material="arrow" amount="64" />
        <potion duration="3">heal</potion>
        <potion duration="3" amplifier="2">damage resistance</potion>
        <leggings unbreakable="true">iron leggings</leggings>
    </kit>
    <kit id="yellow-kit" parents="base-team">
        <item slot="4" amount="64" damage="4">stained clay</item>
        <item slot="5" amount="32" damage="4">stained glass</item>
        <helmet unbreakable="true" color="E5E533">leather helmet</helmet>
        <chestplate unbreakable="true" color="E5E533" enchantment="protection projectile:1">leather chestplate</chestplate>
        <boots unbreakable="true" color="E5E533">leather boots</boots>
    </kit>
    <kit id="blue-kit" parents="base-team">
        <item slot="4" amount="64" damage="11">stained clay</item>
        <item slot="5" amount="32" damage="11">stained glass</item>
        <helmet unbreakable="true" color="0066cc">leather helmet</helmet>
        <chestplate unbreakable="true" color="0066cc" enchantment="protection projectile:1">leather chestplate</chestplate>
        <boots unbreakable="true" color="0066cc">leather boots</boots>
    </kit>
    <kit id="green-kit" parents="base-team">
        <item slot="4" amount="64" damage="5">stained clay</item>
        <item slot="5" amount="32" damage="5">stained glass</item>
        <helmet unbreakable="true" color="667F33">leather helmet</helmet>
        <chestplate unbreakable="true" color="667F33" enchantment="protection projectile:1">leather chestplate</chestplate>
        <boots unbreakable="true" color="667F33">leather boots</boots>
    </kit>
    <kit id="red-kit" parents="base-team">
        <item slot="4" amount="64" damage="14">stained clay</item>
        <item slot="5" amount="32" damage="14">stained glass</item>
        <helmet unbreakable="true" color="CD0000">leather helmet</helmet>
        <chestplate unbreakable="true" color="CD0000" enchantment="protection projectile:1">leather chestplate</chestplate>
        <boots unbreakable="true" color="CD0000">leather boots</boots>
    </kit>

    <kit id="wool-room-chestplate" force="true"> <!-- we need to force this so their chestplate is replaced -->
        <chestplate unbreakable="true" enchantment="protection projectile:1">diamond chestplate</chestplate>
    </kit>
    <kit id="wool-room-kit"> <!-- and we can't have force on this or else they'll get another stack whenever they walk into the woolroom -->
        <item material="golden_apple" amount="24" />
        <item material="arrow" amount="64" />
    </kit>
</kits>
<filters>
    <not id="deny-void">
        <void/>
    </not>
    <not id="not-yellow">
        <team>yellow-team</team>
    </not>
    <not id="not-blue">
        <team>blue-team</team>
    </not>
    <not id="not-green">
        <team>green-team</team>
    </not>
    <not id="not-red">
        <team>red-team</team>
    </not>
    <not id="not-in-yellow-wool-room">
        <any>
            <team>yellow-team</team>
            <material>chest</material>
            <material>clay</material>
            <material>smooth brick</material>
            <material>skull</material>
            <material>wall_sign</material>
            <material>step</material>
            <material>double step</material>
            <material>thin glass</material>
            <material>wool</material>
            <material>redstone wire</material>
            <material>redstone torch on</material>
        </any>
    </not>
    <not id="not-in-blue-wool-room">
        <any>
            <team>blue-team</team>
            <material>chest</material>
            <material>clay</material>
            <material>smooth brick</material>
            <material>skull</material>
            <material>wall_sign</material>
            <material>step</material>
            <material>double step</material>
            <material>thin glass</material>
            <material>wool</material>
            <material>redstone wire</material>
            <material>redstone torch on</material>
        </any>
    </not>
    <not id="not-in-green-wool-room">
        <any>
            <team>green-team</team>
            <material>chest</material>
            <material>clay</material>
            <material>smooth brick</material>
            <material>skull</material>
            <material>wall_sign</material>
            <material>step</material>
            <material>double step</material>
            <material>thin glass</material>
            <material>wool</material>
            <material>redstone wire</material>
            <material>redstone torch on</material>
        </any>
    </not>
    <not id="not-in-red-wool-room">
        <any>
            <team>red-team</team>
            <material>chest</material>
            <material>clay</material>
            <material>smooth brick</material>
            <material>skull</material>
            <material>wall_sign</material>
            <material>step</material>
            <material>double step</material>
            <material>thin glass</material>
            <material>wool</material>
            <material>redstone wire</material>
            <material>redstone torch on</material>
        </any>
    </not>
    <material id="only-glass-panes">thin glass</material>
    <deny id="no-safeties">
        <material>wool</material>
    </deny>
</filters>
<regions>
    <above id="heightlimit" y="28" />
    <apply block="never" region="heightlimit" message="You may not build above the height limit!" />
    <union id="spawns">
        <cuboid id="yellow-spawn" min="100,0,-59" max="111,20,-43" />
        <cuboid id="blue-spawn" min="196,0,-47" max="213,20,-36" />
        <cuboid id="green-spawn" min="106,0,-155" max="123,20,-144" />
        <cuboid id="red-spawn" min="208,0,-149" max="219,20,-132" />
    </union>
    <apply block-place="never" block-break="only-glass-panes" region="spawns" message="You may not build in spawn!" />
    <union id="iron-regen">
        <cuboid id="yellow-iron" min="100,20,-59" max="111,27,-43" />
        <cuboid id="blue-iron" min="196,20,-47" max="213,27,-36" />
        <cuboid id="green-iron" min="106,20,-155" max="123,27,-144" />
        <cuboid id="red-iron" min="208,20,-149" max="219,27,-132" />
    </union>
    <union id="woolrooms">
        <rectangle id="yellow-woolroom" min="137,-59" max="152,-48" />
        <rectangle id="blue-woolroom" min="196,-88" max="207,-73" />
        <rectangle id="green-woolroom" min="112,-118" max="123,-103" />
        <rectangle id="red-woolroom" min="167,-143" max="182,-132" />
    </union>
    <apply kit="wool-room-chestplate" region="woolrooms" />
    <apply kit="wool-room-kit" region="woolrooms" />
    <apply enter="not-yellow" region="yellow-woolroom" message="You may not enter your own woolroom!" />
    <apply enter="not-blue" region="blue-woolroom" message="You may not enter your own woolroom!" />
    <apply enter="not-green" region="green-woolroom" message="You may not enter your own woolroom!" />
    <apply enter="not-red" region="red-woolroom" message="You may not enter your own woolroom!" />
    <apply block="not-in-yellow-wool-room" region="yellow-woolroom" />
    <apply block="not-in-blue-wool-room" region="blue-woolroom" />
    <apply block="not-in-green-wool-room" region="green-woolroom" />
    <apply block="not-in-red-wool-room" region="red-woolroom" />
    <apply block-place="no-safeties" region="everywhere" message="You may not place safety wools! They can only be placed at the victory monument!" />
    <!-- void stuff -->
    <complement id="void-area">
        <everywhere/>
        <rectangle min="137,-118" max="182,-73" /> <!-- mid islands -->

        <rectangle min="114,-88" max="137,-73" /> <!-- yellow to mid -->
        <rectangle min="167,-74" max="182,-50" /> <!-- blue to mid -->
        <rectangle min="137,-118" max="152,-141" /> <!-- green to mid -->
        <rectangle min="181,-118" max="205,-103" /> <!-- red to mid -->

        <rectangle min="100,-65" max="110,-60" /> <!-- in front of yellow -->
        <rectangle min="190,-37" max="195,-47" /> <!-- in front of blue -->
        <rectangle min="124,-155" max="129,-145" /> <!-- in front of green -->
        <rectangle min="208,-131" max="218,-126" /> <!-- in front of red -->
    </complement>
    <apply block="deny-void" region="void-area" message="You may not build in the void!" />
</regions>
<renewables>
    <renewable region="iron-regen" interval="1s">
        <renew-filter>
            <material>iron block</material>
        </renew-filter>
    </renewable>
</renewables>
<wools team="yellow-team">
    <wool color="red" location="171,14,-138">
        <monument><block>106,7,-50</block></monument>
    </wool>
    <wool color="blue" location="201,14,-84">
        <monument><block>105,7,-50</block></monument>
    </wool>
    <wool color="green" location="117,14,-108">
        <monument><block>104,7,-50</block></monument>
    </wool>
</wools>
<wools team="blue-team">
    <wool color="red" location="171,14,-138">
        <monument><block>205,7,-43</block></monument>
    </wool>
    <wool color="yellow" location="147,14,-54">
        <monument><block>205,7,-42</block></monument>
    </wool>
    <wool color="green" location="117,14,-108">
        <monument><block>205,7,-41</block></monument>
    </wool>
</wools>
<wools team="green-team">
    <wool color="red" location="171,14,-138">
        <monument><block>113,7,-149</block></monument>
    </wool>
    <wool color="blue" location="201,14,-84">
        <monument><block>113,7,-150</block></monument>
    </wool>
    <wool color="yellow" location="147,14,-54">
        <monument><block>113,7,-151</block></monument>
    </wool>
</wools>
<wools team="red-team">
    <wool color="yellow" location="147,14,-54">
        <monument><block>212,7,-142</block></monument>
    </wool>
    <wool color="blue" location="201,14,-84">
        <monument><block>213,7,-142</block></monument>
    </wool>
    <wool color="green" location="117,14,-108">
        <monument><block>214,7,-142</block></monument>
    </wool>
</wools>
<destroyables materials="stained glass" completion="0%" required="false" show="false" mode-changes="true">
    <destroyable name="build-regions" owner="yellow-team">
        <region>
            <cuboid min="86,0,-169" max="232,1,-23" />
        </region>
    </destroyable>
</destroyables>
<modes>
    <mode after="0s" material="air" />
</modes>
<killreward>
	<filter>
		<team>yellow-team</team>
	</filter>
	<item amount="32" damage="4">stained clay</item>
	<item amount="16" damage="4">stained glass</item>
	<item>golden apple</item>
</killreward>
<killreward>
	<filter>
		<team>blue-team</team>
	</filter>
	<item amount="32" damage="11">stained clay</item>
	<item amount="16" damage="11">stained glass</item>
	<item>golden apple</item>
</killreward>
<killreward>
	<filter>
		<team>green-team</team>
	</filter>
	<item amount="32" damage="5">stained clay</item>
	<item amount="16" damage="5">stained glass</item>
	<item>golden apple</item>
</killreward>
<killreward>
	<filter>
		<team>red-team</team>
	</filter>
	<item amount="32" damage="14">stained clay</item>
	<item amount="16" damage="14">stained glass</item>
	<item>golden apple</item>
</killreward>
<toolrepair>
	<tool>iron sword</tool>
	<tool>bow</tool>
	<tool>iron pickaxe</tool>
	<tool>iron axe</tool>
</toolrepair>
<itemremove>
	<item>golden apple</item>
	<item>wool</item>
	<item>stained clay</item>
	<item>stained glass</item>
	<item>glass</item>
	<item>string</item>
	<item>golden carrot</item>
	<item>leather helmet</item>
	<item>leather chestplate</item>
	<item>iron leggings</item>
	<item>leather boots</item>
	<item>glowstone dust</item>
	<item>clay ball</item>
	<item>leaves</item>
	<item>sapling</item>
	<item>arrow</item>
</itemremove>
<respawn delay="4s" />
</map>
