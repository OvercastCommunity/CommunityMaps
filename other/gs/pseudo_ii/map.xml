<teams>
    <team id="green-team" color="green" max="8">Green</team>
    <team id="blue-team" color="blue" max="8">Blue</team>
</teams>
<spawns>
    <spawn team="green-team" yaw="180">
        <region>
           <cylinder base="3,2,12" radius="1" height="0"/>
        </region>
    </spawn>
    <spawn team="blue-team">
        <region>
            <cylinder base="3,2,-36" radius="1" height="0"/>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <cylinder base="-7,13,-12" radius="1" height="0"/>
        </region>
    </default>
</spawns>
<regions>
    <cuboid id="blue-west-portal" min="-8,9,-39" max="-5,12,-40"/>
    <mirror id="blue-east-portal" region="blue-west-portal" normal="1,0,0" origin="3,1,-12"/>
    <mirror id="green-west-portal" region="blue-west-portal" normal="0,0,1" origin="3,1,-12"/>
    <mirror id="green-east-portal" region="blue-east-portal" normal="0,0,1" origin="3,1,-12"/>
    <union id="jump-pads">
        <block id="blue-west-jump-pad">-7,2,-37</block>
        <mirror id="blue-east-jump-pad" region="blue-west-jump-pad" normal="1,0,0" origin="3,1,-12"/>
        <mirror id="green-west-jump-pad" region="blue-west-jump-pad" normal="0,0,1" origin="3,1,-12"/>
        <mirror id="green-east-jump-pad" region="blue-east-jump-pad" normal="0,0,1" origin="3,1,-12"/>
    </union>
    <apply region="jump-pads" velocity="0,1.2,0"/>
</regions>
<portals>
    <portal region="blue-west-portal" x="19"/>
    <portal region="blue-east-portal" x="-19"/>
    <portal region="green-west-portal" x="19"/>
    <portal region="green-east-portal" x="-19"/>
</portals>
</map>
