<map proto="1.5.0">
<name>Battleline</name>
<version>1.0.4</version>
<include id="gapple-kill-reward"/>
<objective>Control the line to win!</objective>
<authors>
    <author uuid="c25a6f7b-4c42-40da-8cd6-add53f0c84eb"/> <!-- arcadeboss -->
</authors>
<teams>
    <team id="blue-team" color="blue" max="30">Blue</team>
    <team id="red-team" color="red" max="30">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" material="stone sword" unbreakable="true"/>
        <item slot="1" material="bow" unbreakable="false"/>
        <item slot="2" material="arrow" amount="8"/>
        <item slot="8" material="golden apple" amount="1"/>
        <chestplate material="leather chestplate" team-color="true" unbreakable="true"/>
        <leggings material="leather leggings" team-color="true" enchantment="protection;projectile_protection" unbreakable="true"/>
        <boots material="iron boots" unbreakable="true"/>
        <effect duration="5s">regeneration</effect>
        <effect duration="3s" amplifier="255">damage resistance</effect>
        <effect duration="5s">slowness</effect>
    </kit>
</kits>
<spawns>
    <default yaw="0" pitch="55">
        <region>
            <point>0.5,57,-21.5</point>
        </region>
    </default>
    <spawn team="blue-team" kit="spawn-kit" yaw="90" region="blue-spawn-point"/>
    <spawn team="red-team" kit="spawn-kit" yaw="-90" region="red-spawn-point"/>
</spawns>
<respawn auto="true" delay="1s"/>
<time result="score">3m</time>
<score/>
<broadcasts>
    <alert after="1s">`r`fThere are 99 separate control points along the line!`r</alert>
    <tip after="9s">`r`fThe capture area for each point is 3 blocks wide!`r</tip>
</broadcasts>
<variables>
    <with-team id="blue_score" var="score" team="blue-team"/>
    <with-team id="red_score" var="score" team="red-team"/>
    <variable id="score_diff" scope="match"/>
</variables>
<actions>
    <trigger scope="match" filter="score-track">
        <action>
            <set var="score_diff" value="red_score-blue_score"/>
        </action>
    </trigger>
    <trigger scope="player" filter="help-red">
        <action filter="red-team">
            <kit force="true">
                <effect duration="3s" amplifier="1">strength</effect>
            </kit>
        </action>
    </trigger>
    <trigger scope="player" filter="help-blue">
        <action filter="blue-team">
            <kit force="true">
                <effect duration="3s" amplifier="1">strength</effect>
            </kit>
        </action>
    </trigger>
    <trigger filter="match-started" scope="match">
        <action>
            <fill region="line" material="stained_clay:0"/>
            <fill region="glass" material="stained_glass:0"/>
            <fill region="carpet" material="carpet:0"/>
        </action>
    </trigger>
</actions>
<filters>
    <pulse id="score-track" period="3s" duration="0.5s" filter="always"/>
    <pulse id="help-red" period="9s" duration="0.5s" filter="all(score_diff=..0,not(score_diff=0))"/>
    <pulse id="help-blue" period="9s" duration="0.5s" filter="all(score_diff=0..,not(score_diff=0))"/>
</filters>
<regions>
