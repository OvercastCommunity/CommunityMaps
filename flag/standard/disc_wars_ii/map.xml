<map proto="1.4.2">
<name>Disc Wars II</name>
<version>1.0</version>
<objective>Capture the enemy's flag as many times as possible in 12 minutes to win!</objective>
<gamemode>ctf</gamemode>
<gamemode>rage</gamemode>
<created>2022-07-03</created>
<time result="score" overtime="1m" max-overtime="2m">12m</time>
<authors>
    <author uuid="4011d739-4dfb-41f6-8e60-5179d035cab7" contribution="Layout, aesthetics, concept"/> <!-- MishM0sh -->
    <author uuid="6863869b-4b8c-4445-b778-a8e016775ae4" contribution="General assistance, XML"/> <!-- Cazadorian -->
</authors>
<contributors>
    <contributor uuid="59655bfd-1fda-4e15-b6f7-eb6b6f84b406" contribution="Extensive map feedback"/> <!-- Digitalight -->
</contributors>
<rules>
    <rule>Fall damage is disabled!</rule>
</rules>
<score>
    <mercy>3</mercy>
</score>
<rage/>
<teams>
    <team id="red" color="dark_red" max="12">Red</team>
    <team id="blue" color="blue" max="12">Blue</team>
</teams>
<spawns>
    <default kit="default-kit">
        <region yaw="180">
            <cylinder base="0.5,104,0.5" radius="1" height="0"/>
        </region>
    </default>
    <spawn team="blue" kit="blue-kit">
        <region yaw="0">
            <cylinder base="0.5,31,-41.5" radius="1" height="0"/>
        </region>
    </spawn>
    <spawn team="red" kit="red-kit">
        <region yaw="180">
            <cylinder base="0.5,31,42.5" radius="1" height="0"/>
        </region>
    </spawn>
</spawns>
<kits>
    <kit id="default-kit">
        <effect>night vision</effect>
    </kit>
    <kit id="spawn-kit">
        <clear/>
        <item slot="1" unbreakable="true" name="`r`lLaser Rifle" enchantment="arrow damage:10;damage all:10" lore="`aInsta kill!">bow</item>
        <item slot="2" amount="2">arrow</item>
        <effect duration="3" amplifier="100">regeneration</effect>
        <effect duration="3" amplifier="100">damage resistance</effect>
        <effect>speed</effect>
        <chestplate material="leather chestplate" name="`r`lDigitalized Armour" team-color="true" unbreakable="true"/>
        <leggings material="leather leggings" name="`r`lDigitalized Armour" team-color="true" unbreakable="true"/>
        <boots material="leather boots" name="`r`lDigitalized Armour" team-color="true" unbreakable="true"/>
        <game-mode>adventure</game-mode>
        <effect>night vision</effect>
    </kit>
    <kit id="red-kit" parents="spawn-kit">
        <item slot="0" unbreakable="true" name="`r`c`lIdentity Disc" enchantment="damage all:10" lore="`aInsta kill!">record 4</item>
        <helmet material="stained glass" locked="true" damage="14" name="`r`c`lDigitalized Armour" enchantment="protection environmental:4"/>
    </kit>
    <kit id="blue-kit" parents="spawn-kit">
        <item slot="0" unbreakable="true" name="`r`9`lIdentity Disc" enchantment="damage all:10" lore="`aInsta kill!">record 12</item>
        <helmet material="stained glass" locked="true" damage="11" name="`r`9`lDigitalized Armour" enchantment="protection environmental:4"/>
    </kit>
</kits>
<kill-reward>
    <item amount="2" material="arrow"/>
</kill-reward>
<toolrepair>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>record 4</item>
    <item>record 12</item>
    <item>stained glass</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>arrow</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<respawn delay="2.5s"/>
<filters>
    <team id="only-blue">blue</team>
    <team id="only-red">red</team>
    <deny id="no-beacon">
        <material>beacon</material>
    </deny>
</filters>
<regions>
    <union id="jump-pads">
        <union id="red-pads">
            <cylinder id="red-east" base="18.5,32,42.5" radius="2" height="0.5"/>
            <cylinder id="red-west" base="-17.5,32,42.5" radius="2" height="0.5"/>
        </union>
        <union id="blue-pads">
            <cylinder id="blue-east" base="18.5,32,-41.5" radius="2" height="0.5"/>
            <cylinder id="blue-west" base="-17.5,32,-41.5" radius="2" height="0.5"/>
        </union>
        <union id="middle-pads">
            <union id="bottom-pads">
                <cylinder id="nw-bottom-pad" base="-20.5,36,-18.5" radius="2" height="0.5"/>
                <cylinder id="ne-bottom-pad" base="21.5,36,-18.5" radius="2" height="0.5"/>
                <cylinder id="sw-bottom-pad" base="-20.5,36,19.5" radius="2" height="0.5"/>
                <cylinder id="se-bottom-pad" base="21.5,36,19.5" radius="2" height="0.5"/>
                <cylinder id="middle-pad" base="0.5,37,0.5" radius="2" height="0.5"/>
            </union>
            <union id="side-pads">
                <cylinder id="nw-top-pad" base="-30.5,47,-33.5" radius="2" height="0.5"/>
                <cylinder id="ne-top-pad" base="31.5,47,-33.5" radius="2" height="0.5"/>
                <cylinder id="sw-top-pad" base="-30.5,47,34.5" radius="2" height="0.5"/>
                <cylinder id="se-top-pad" base="31.5,47,34.5" radius="2" height="0.5"/>
                <cylinder id="west-to-mid" base="-32.5,46,0.5" radius="2" height="0.5"/>
                <cylinder id="east-to-mid" base="33.5,46,0.5" radius="2" height="0.5"/>
            </union>
        </union>
    </union>
    <apply velocity="@-5,2,0" region="red-west" filter="only-red"/>
    <apply velocity="@-5,2,0" region="blue-west" filter="only-blue"/>
    <apply velocity="@-3,1.8,0" region="nw-bottom-pad"/>
    <apply velocity="@-3,1.8,0" region="sw-bottom-pad"/>
    <apply velocity="@5,2,0" region="red-east" filter="only-red"/>
    <apply velocity="@5,2,0" region="blue-east" filter="only-blue"/>
    <apply velocity="@3,1.8,0" region="ne-bottom-pad"/>
    <apply velocity="@3,1.8,0" region="se-bottom-pad"/>
    <apply velocity="@0,2.7,0" region="middle-pad"/>
    <apply velocity="@-3,1,0" region="ne-top-pad"/>
    <apply velocity="@-3,1,0" region="se-top-pad"/>
    <apply velocity="@3,1,0" region="nw-top-pad"/>
    <apply velocity="@3,1,0" region="sw-top-pad"/>
    <apply velocity="@-5,2.5,0" region="east-to-mid"/>
    <apply velocity="@5,2.5,0" region="west-to-mid"/>
    <apply block="never" use="no-beacon"/>
</regions>
<flags flag-proximity-metric="none" net-proximity-metric="none">
    <flag id="blue-flag" name="Blue Flag" owner="blue" color="blue" pickup-filter="only-red">
        <post recover-time="15s" respawn-time="10" yaw="180">0.5,45,-29.5</post>
    </flag>
    <net points="1" flag="blue-flag" return="red-flag">
        <region>
            <cylinder base="0.5,45,30.5" radius="2" height="2"/>
        </region>
    </net>
    <flag id="red-flag" name="Red Flag" owner="red" color="red" pickup-filter="only-blue">
        <post recover-time="15s" respawn-time="10" yaw="0">0.5,45,30.5</post>
    </flag>
    <net points="1" flag="red-flag" return="blue-flag">
        <region>
            <cylinder base="0.5,45,-29.5" radius="2" height="2"/>
        </region>
    </net>
</flags>
</map>