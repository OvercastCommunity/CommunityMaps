<spawners max-entities="1">
    <spawner spawn-region="spawner1" player-region="spawner1-radius">
        <item material="golden apple"/>
    </spawner>
    <spawner spawn-region="spawner2" player-region="spawner2-radius">
        <item material="golden apple"/>
    </spawner>
    <spawner spawn-region="spawner3" player-region="spawner3-radius">
        <item material="golden apple"/>
    </spawner>
    <spawner spawn-region="spawner4" player-region="spawner4-radius">
        <item material="golden apple"/>
    </spawner>
</spawners>
<flags>
    <flag id="flag"
          name="Flag"
          color="orange"
          shared="true"
          post="post_upper"
          points-rate="1"
          pickup-kit="flag-pickup"
          drop-kit="reset-health"
          flag-proximity-metric="none"
          show-respawn-on-pickup="true">
        <post id="post_upper" return-time="0s" respawn-time="15s" name="Top Platform">0,26,0</post>
        <post id="post_lower" return-time="0s" respawn-time="15s" name="Bottom Platform">0,13,0</post>
        <post id="post_side" return-time="0s" respawn-time="15s" name="Green and Magenta side">30,19,0</post>
        <post id="post_side_lobby" return-time="0s" respawn-time="15s" name="Red and Cyan side">-30,19,0</post>
    </flag>
</flags>
<score>
    <limit>200</limit>
</score>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>iron boots</item>
</itemremove>
<itemkeep>
    <item>golden apple</item>
    <item>arrow</item>
</itemkeep>
<kill-reward>
    <item material="golden apple"/>
    <item amount="6" material="arrow"/>
</kill-reward>
<respawn spectate="true">
    <message>
        {"translate":"death.respawn.confirmed.waiting.flagDropped"}
    </message>
</respawn>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<hunger>
