    <cylinder id="spawner3-radius" base="-29.5,15,-16.5" height="5" radius="2.5"/>
    <cuboid id="spawner4" min="-32,15.5,16" max="-27,17,19"/>
    <cylinder id="spawner4-radius" base="-29.5,15,17.5" height="5" radius="2.5"/>
