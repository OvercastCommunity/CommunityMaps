<?xml version="1.0"?>
<map proto="1.4.2">
<name>Focus</name>
<version>0.0.1</version>
<gamemode>ctf</gamemode>
<gamemode>rage</gamemode>
<objective>Capture the enemy's flag 5 times to win!</objective>
<authors>
    <author uuid="834704b6-94d2-4f4f-abc6-fe2e1f085837" /> <!-- Alternqtive -->
</authors>
<contributors>
    <contributor uuid="dad8b95c-cf6a-44df-982e-8c8dd70201e0" contribution="Directrix inspiration"/> <!-- ElectroidFilms -->
</contributors>
<rules>
    <rule>Fall damage is disabled.</rule>
</rules>
<teams>
    <team id="red" color="dark red" max="12">Red</team>
    <team id="blue" color="blue" max="12">Blue</team>
</teams>
<flags>
    <flag id="red-flag" name="Red Flag" owner="red" color="red" pickup-filter="only-blue">
        <post yaw="-180">13.5,8,-64.5</post>
        <net points="1">
            <region>
                <cylinder base="-12.5,20,68.5" radius="3" height="2"/>
            </region>
            <capture-filter>
                <not>
                    <flag-carried>blue-flag</flag-carried>
                </not>
            </capture-filter>
        </net>
    </flag>
    <flag id="blue-flag" name="Blue Flag" owner="blue" color="blue" pickup-filter="only-red" return-time="45s">
        <post yaw="0">-12.5,8,65.5</post>
        <net points="1">
            <region>
                <cylinder base="13.5,20,-67.5" radius="3" height="2"/>
            </region>
            <capture-filter>
                <not>
                    <flag-carried>red-flag</flag-carried>
                </not>
            </capture-filter>
        </net>
    </flag>
</flags>
<score>
    <limit>5</limit>
</score>
<spawns>
    <spawn team="red" kit="red-kit" spread="false">
        <regions>
            <point yaw="0">13.5,21,-67.5</point>
        </regions>
    </spawn>
    <spawn team="blue" kit="blue-kit" spread="false">
        <regions>
            <point yaw="180">-12.5,21,68.5</point>
        </regions>
    </spawn>
    <default kit="all">
        <regions yaw="90" pitch="-5">
            <point>-59.5,24,0.5</point>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="all">
        <potion duration="oo">night vision</potion>
    </kit>
    <kit id="teams" parents="all">
        <item slot="0" name="`aCutlass" unbreakable="true" enchantment="damage all:10">stone sword</item>
        <item slot="1" name="`bInstagib" unbreakable="true" enchantment="arrow damage:10">bow</item>
        <item slot="8" amount="3">arrow</item>
        <chestplate unbreakable="true" locked="true">iron chestplate</chestplate>
        <leggings color="191919" unbreakable="true" locked="true">chainmail leggings</leggings>
        <boots color="191919" unbreakable="true" locked="true">leather boots</boots>
        <potion duration="1.5" amplifier="5">damage resistance</potion>
        <potion duration="1.5">absorption</potion>
        <potion duration="oo">speed</potion>
        <game-mode>adventure</game-mode>
    </kit>
    <kit id="red-kit" parents="teams">
        <helmet color="993333" unbreakable="true" locked="true">leather helmet</helmet>
    </kit>
    <kit id="blue-kit" parents="teams">
        <helmet color="334CB2" unbreakable="true" locked="true">leather helmet</helmet>
    </kit>
</kits>
<filters>
    <team id="only-red">red</team>
    <team id="only-blue">blue</team>
</filters>
<regions>
    <union id="jump-pads">
        <union id="red-jump-pads">
            <cuboid id="red-spawn-left" min="33,19,-42" max="30,20,-40"/> <!-- +x -->
            <cuboid id="red-spawn-right" min="-15,19,-24" max="-18,20,-22"/> <!-- +z -->
            <cuboid id="red-bottom-left" min="26,8,-66" max="28,9,-63"/> <!-- +z -->
            <cuboid id="red-bottom-front" min="15,8,-38" max="12,9,-36"/> <!-- -x -->
            <cuboid id="red-bottom-right" min="-18,8,-45" max="-19,9,-49"/> <!-- +z -->
            <cuboid id="red-bottom-high" min="-38,8,-17" max="-42,9,-15"/> <!-- +z -->
            <cuboid id="red-top-over" min="42,31,-46" max="40,32,-49"/> <!-- +z -->
        </union>
        <union id="blue-jump-pads">
            <cuboid id="blue-spawn-left" min="-32,19,43" max="-29,20,41"/> <!-- +x -->
            <cuboid id="blue-spawn-right" min="16,19,25" max="19,20,23"/> <!-- +z -->
            <cuboid id="blue-bottom-left" min="-25,8,67" max="-27,9,64"/> <!-- +z -->
            <cuboid id="blue-bottom-front" min="-14,8,39" max="-11,9,37"/> <!-- -x -->
            <cuboid id="blue-bottom-right" min="18,8,47" max="20,9,50"/> <!-- +z -->
            <cuboid id="blue-bottom-high" min="39,8,18" max="43,9,16"/> <!-- +z -->
            <cuboid id="blue-top-over" min="-41,31,47" max="-39,32,50"/> <!-- +z -->
        </union>
        <union id="middle-jump-pads">
            <cuboid id="mid-to-east" min="22,8,-1" max="24,9,2"/> <!-- -x -->
            <cuboid id="mid-to-west" min="-21,8,2" max="-23,9,-1"/> <!-- +x -->
            <cuboid id="east-to-top" min="54,25,2" max="52,26,-1"/> <!-- -x +y -->
            <cuboid id="west-to-top" min="-53,25,-1" max="-51,26,2"/> <!-- +x +y -->
            <cuboid id="top-to-red" min="-1,40,-10" max="2,41,-12"/> <!-- +x +y -->
            <cuboid id="top-to-blue" min="2,40,11" max="-1,41,13"/> <!-- +x +y -->
            <cuboid id="top-only-red" min="-38,40,8" max="-42,41,10"/> <!-- +x +y -->
            <cuboid id="top-only-blue" min="39,40,-7" max="43,41,-9"/> <!-- +x +y -->

        </union>
    </union>
    <!-- Red pads -->
    <apply enter="only-red" velocity="@0,3,6" region="red-spawn-left"/>
    <apply velocity="@0,2.7,2.5" region="red-spawn-right"/>
    <apply velocity="@2.5,2.5,0" region="red-bottom-left"/>
    <apply velocity="@0,1.5,6" region="red-bottom-front"/>
    <apply velocity="@-2.7,1.3,0" region="red-bottom-right"/>
    <apply velocity="@0,3.1,2" region="red-bottom-high"/>
    <apply velocity="@-7,1.7,0" region="red-top-over"/>
    <!-- Blue pads -->
    <apply enter="only-blue" velocity="@0,3,-6" region="blue-spawn-left"/>
    <apply velocity="@0,2.7,-2.5" region="blue-spawn-right"/>
    <apply velocity="@-2.5,2.5,0" region="blue-bottom-left"/>
    <apply velocity="@0,1.5,-6" region="blue-bottom-front"/>
    <apply velocity="@2.7,1.3,0" region="blue-bottom-right"/>
    <apply velocity="@0,3.1,-2" region="blue-bottom-high"/>
    <apply velocity="@0,3.1,-2" region="blue-top-over"/>
    <!-- Mid pads -->
    <apply velocity="@6.5,2.3,0" region="mid-to-east"/>
    <apply velocity="@-6.5,2.3,0" region="mid-to-west"/>
    <apply velocity="@-1.3,2.4,0" region="east-to-top"/>
    <apply velocity="@1.3,2.4,0" region="west-to-top"/>
    <apply velocity="@0,1,-2.6" region="top-to-red"/>
    <apply velocity="@0,1,2.6" region="top-to-blue"/>
    <apply enter="only-red" velocity="@0,1,1.5" region="top-only-red"/>
    <apply enter="only-blue" velocity="@0,1,-1.5" region="top-only-blue"/>
    <apply block="never" region="everywhere"/>
</regions>
<time result="objectives">15m</time>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>bow</item>
    <item>stone sword</item>
</itemremove>
<killreward>
    <filter>
        <kill-streak count="1" repeat="true"/>
    </filter>
    <item amount="3">arrow</item>
</killreward>
<gamerules>
    <doDaylightCycle>false</doDaylightCycle>
</gamerules>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
<rage/>
<respawn delay="3.5s"/>
</map>