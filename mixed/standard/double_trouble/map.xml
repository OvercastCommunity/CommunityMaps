        <cuboid min="-111,40,128"   max="113,45,130" />
        <cuboid min="111,9,128"  max="113,14,130"/>
        <cuboid min="155,22,109"   max="158,28,110" />
        <cuboid min="158,22,96"  max="155,28,95"/>
