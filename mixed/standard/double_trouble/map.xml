    <apply region="blue-second-cuboid-inside" enter="only-red"                                    message="`cYou may not enter `4Red`c's woolroom!" />
    <apply region="red-second-cuboid-inside"  enter="only-blue"                                   message="`cYou may not enter `9Blue`c's woolroom!" />
