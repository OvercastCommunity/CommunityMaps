        <cuboid min="-15,40,-110"   max="-17,45,-112" />
        <cuboid min="-15,9,-110"  max="-17,14,-112"/>
        <cuboid min="-59,22,-91"   max="-62,28,-92" />
        <cuboid min="-62,22,-78"  max="-59,28,-77"/>
    </union>
    <complement id="wool-not-allowed-here">
        <region id="playable-area" />
        <region id="wool-monuments" />
    </complement>
    <!-- Block Rules -->
    <apply region="spawns" block="never" message="You may not edit spawn!"/>
    <apply region="portal-regions"                         kit="remove-effects" />
    <apply region="above-spawn"          block="never"                                     message="You may not climb above the map boundaries" />
    <apply region="outside-playable-area"                  block="never"                   messgae="You may not build outside of the map boundaries" />
    <apply region="blue-teleport-platforms" enter="only-blue" block="never"                   message="`cYou may not interact with `9Blue`c's spawn" />
    <apply region="blue-filter-portals" enter="only-blue" block="never"                   message="`cYou may not interact with `9Blue`c's spawn" />
    <apply region="blue-protection"                      block="tnt-immune"                   message="`cYou may not interact with `9Blue`c's spawn" />
    <apply region="red-teleport-platforms"  enter="only-red"  block="never"                   message="`cYou may not interact with `4Red`c's spawn" />
