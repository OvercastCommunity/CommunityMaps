        <destination>
            <region id="return-blue-spawn"/>
        </destination>
    </portal>
    <portal yaw="@90">
        <region>
            <cuboid id="blue-down"  min="-15,9,-110"  max="-17,14,-112"/>
        </region>
        <destination>
